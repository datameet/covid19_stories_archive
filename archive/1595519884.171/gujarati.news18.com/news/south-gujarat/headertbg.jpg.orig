    function nw_click_refresh_election_data() {
        $(document).on('click', '.refeshdata', function () {

            nw_hit_election_ajax_direct();
        });
    }

    function nw_refresh_election_data() {
        setInterval(function () {
            if (run_ajax == true) {

                nw_hit_election_ajax_direct();
            }
        }, 180000);
    }
	
	function nw_get_exit_poll_status(){
		var url = 'https://election.nw18.com/electiondata/electionjson/exitpoll/exitpoll-status.json';
        $.ajax({
            url: url,
            type: 'GET',
			dataType: 'json',
            //jsonp: 'jsonp',
            crossDomain: true,
            cache: true,
            success: function (response) {
				var is_exit_poll_on = response.is_exitpoll_live;
				if(is_exit_poll_on==1){
					nw_hit_election_ajax_color_codes();
				}                
            }
        });
	}

    function nw_hit_election_ajax_color_codes() {
        var url = 'https://election.nw18.com/electiondata/electionjson/general_election_2019/lok_sabha/party_colour.json';
        $.ajax({
            url: url,
            type: 'GET',
            crossDomain: true,
            cache: true,
            success: function (response) {
                $.each(response, function (party_slug, party_details) {
                    color_codes[party_slug] = party_details['PARTY_COLOUR'];
                });
                nw_hit_election_ajax_direct();
                nw_refresh_election_data();
            }
        });
    }

    function nw_hit_election_ajax_direct() {
        var url = 'https://election.nw18.com/electiondata/electionjson/manual_lok_sabha_2019_exit_poll_result_cross.json';
        var callbackjsonp = 'loc_sabha_election_2019';
        $.ajax({
            url: url,
            type: 'GET',
            dataType: 'jsonp',
            jsonp: 'jsonp',
            jsonpCallback: callbackjsonp,
            crossDomain: true,
            cache: true,
            data: {
                action: 'nw_refresh_data',
            },
            success: function (response) {
                response = response.result;

                var election_data = nw_get_election_data(response);

                var loksabha_data = election_data['loksabha'];
                var assembly_data = election_data['assembly'];

                var loksabha_data_html = nw_get_loksabha_html(loksabha_data);
                var assembly_data_html = nw_get_assembly_html(assembly_data);

                var final_html = '<div class="global-exitpollwrap">';
                final_html += '<div class="global-exitpoll clearfix">' + loksabha_data_html + assembly_data_html;
                final_html += '</div></div>';

                $('.nw_section_ajax_refresh').html(final_html);

                // first destroy slicks
                //$('.loksabha-exitpoll').slick('destroy');
                //$('.assembly-exitpoll').slick('destroy');

                // reinitiate slick
                $('.loksabha-exitpoll').slick({
                    dots: true,
                    infinite: true,
                    speed: 1000,
                    slidesToShow: 1,
                    variableWidth: true,
                    autoplay: true,
                    slidesToScroll: 1,
                    responsive: [{
                            breakpoint: 650,
                            settings: {
                                slidesToShow: 1,
                                slidesToScroll: 1,
                            }
                        }]

                });

                $('.assembly-exitpoll').slick({
                    dots: true,
                    infinite: true,
                    speed: 500,
                    slidesToShow: 1,
                    autoplay: false,
                
                });
				
            }
        });
    }

    function nw_get_election_data(response) {
        var final_data = new Array();
        var loksabha_data = new Array();
        var assembly_data = new Array();

        for (var key in response) {
            if (response.hasOwnProperty(key)) {
                var $cons_details = response[key];

                if ($cons_details['cons_type'] == 'loc') {
                    loksabha_data[$cons_details['display_order']] = $cons_details;
                    continue;
                }

                if ($cons_details['cons_type'] == 'as') {
                    assembly_data[$cons_details['display_order']] = $cons_details;
                    continue;
                }
            }
        }

