var bypoll_party_summary = function(data){
	let byPollWidget = document.getElementById("by-poll-widget");
	if (true){
		 
		byPollWidget.style.display = "block";
	}
	else{
		 
		byPollWidget.style.display = "block";
	}
	let headerHtml = '';
	let innerHtml = '';
	let bottamHtml = '';
	let i = 0;
	let glideBtn = '';
	for (let key in data) {
		let item = data[key];
		if (item.Assembly != undefined){
			for (let ky in item.Assembly.data) {
				let itm = item.Assembly.data[ky];
				headerHtml += `<th class="newelecparty bjp" style="background:` +  itm.color + `">` + itm.name + `</th>`;
				bottamHtml += `<td>` + itm.wonlead + `</td>`;
			}
			innerHtml += `<li>
							<div class="newelecbypoll-sliderhd">
							<h3>` + item.Assembly.state_name +`<span>(ASSEMBLY)</span></h3>
							<p>` + item.Assembly.total_seats.wonlead + `/<span>` + item.Assembly.total_seats.seats +` Seat</span></p>
							</div>
							<table cellspacing="0" cellpadding="0">
							<tr> ` + headerHtml +`</tr>
							<tr>` + bottamHtml + ` </tr>
							</table>
						</li>`;
			headerHtml = '';
			bottamHtml = '';
		}

		if (item['Lok Sabha'] != undefined){
			let mainItem = item['Lok Sabha'];
			for (let ky in mainItem.data) {
				let itm = mainItem.data[ky];
				headerHtml += `<th class="newelecparty bjp" style="background:` +  itm.color + `">` + itm.name + `</th>`;
				bottamHtml += `<td>` + itm.wonlead + `</td>`;
			}
			innerHtml += `<li>
							<div class="newelecbypoll-sliderhd">
							<h3>` + mainItem.state_name +`<span>(Lok Sabha)</span></h3>
							<p>` + mainItem.total_seats.wonlead + `/<span>` + mainItem.total_seats.seats +` Seat</span></p>
							</div>
							<table cellspacing="0" cellpadding="0">
							<tr> ` + headerHtml +`</tr>
							<tr>` + bottamHtml + ` </tr>
							</table>
						</li>`;
			headerHtml = '';
			bottamHtml = '';
		}

		glideBtn += `<button data-glide-dir="=` + i++ +`"></button>`;
		}


	innerHtml = `<div class="newelecbypoll-sliderwrap">
            <div data-glide-el="track" >
               <ul  class="newelecbypoll-slider">` + innerHtml + `
                 
               </ul>
               <div class="newelecbypoll-sliderbullet" data-glide-el="controls[nav]">
                ` + glideBtn + `
               </div>
            </div>
         </div>`;


	document.getElementById('el_bypoll_ul').innerHTML = innerHtml;
	new Glide(document.querySelector('.newelecbypoll-sliderwrap'), {
				  autoplay: 2000,type: 'carousel',perView:4,rewind:false,slidesToScroll: 1,breakpoints: {
				  650: {perView: 1.2}}}).mount();	

	

}

var switchdata_mhhr_assembly_2019 = async function(data){
	let timeInterval = 60000;
	let showAllianceAndParty = 'on';
	let showByPoll = 'off';
	if (showAllianceAndParty == 'on'){
		election_assembly_callback("election_assembly_2019_haryana_all", data.seat_party_summary_all);
		setTimeout(function(){
			election_assembly_callback("election_assembly_2019_haryana_all", data.seat_party_summary_all);
		}, 3000);
		var intervalIdAll = window.setInterval(
			election_assembly_callback,
			timeInterval,
			"election_assembly_2019_haryana_all",
			data.seat_party_summary_all
		);
	}
	if (showByPoll == 'on'){
		election_assembly_callback("election_assembly_2019_haryana_all", data.by_poll_party_summary_cross);
		setTimeout(function(){
			election_assembly_callback("election_assembly_2019_haryana_all", data.by_poll_party_summary_cross); 
		}, 3000);
		var intervalByPoll = window.setInterval(
			election_assembly_callback,
			timeInterval,
			"election_assembly_2019_haryana_all",
			data.by_poll_party_summary_cross
		);
	}
	

	return data;
}

async function election_assembly_callback(id, url) {
	let sc = document.createElement("script");
	sc.src = url;
	sc.id = id + (new Date().getTime());
	document.body.appendChild(sc);
	document.getElementById(sc.id).remove();
}
