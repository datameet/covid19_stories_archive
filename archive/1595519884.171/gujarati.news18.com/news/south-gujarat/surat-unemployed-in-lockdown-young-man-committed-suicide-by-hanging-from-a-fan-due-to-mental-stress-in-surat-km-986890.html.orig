			target = target.parentNode;
			if(!target) { return; }
		}
		if (target.tagName === 'LI'){
			target.className = 'active';
			document.getElementById("newelec-tallywidgetwrap-" + target.id + "-tally").style.display = "block";
			if (target.nextElementSibling != null){
