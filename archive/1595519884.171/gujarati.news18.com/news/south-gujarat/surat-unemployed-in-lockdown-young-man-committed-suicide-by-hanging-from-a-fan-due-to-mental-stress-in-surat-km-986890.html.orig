var election_assembly_2019_maharashtra = function(data){
	let headerHtml = '';
	let innerHtml = '';
	let bottamHtml = '';
	let progressbarHtml = '';
	let totalWonSeat = data.total_seats.seats;
	data.alliance.forEach(function(item){
		headerHtml += `<th style="background:` +  item.color + `">` + item.display_name+ `</th>`;
		innerHtml += `<td style="background:` +  item.color + `">` +  item.wonlead + `</td>`;
		bottamHtml +=`<td>` + item.changes + `</td>`
		progressbarHtml +=  `<span style="width:` + (item.wonlead/totalWonSeat*100) + `%; background:` +  item.color + `"></span>`; 
	});

	let html = `<table cellpadding="0" cellspacing="0"><tr>
				<th>Alliance</th> ` + headerHtml + `
				</tr>
				<tr>
				<td>Wins + Leads</td> ` + innerHtml + `
				</tr>
				<tr>
				<td>+/- 2014</td> ` + bottamHtml + `
				</tr>
				</table>
				<div class="newelec-tallywidget-right"></div>`;

	document.getElementById('el_mah_winseat').innerHTML = `(` + (data.total_seats.seats/2 + 1) + ` Seats to Win)`;
	document.getElementById('el_mah_total').innerHTML =  `<span>` + data.total_seats.wonlead + `</span>/ ` + data.total_seats.seats + ` Seats `;
	document.getElementById('el_mah_inner').innerHTML = html;
	document.getElementById('el_mah_pro_bar').innerHTML = progressbarHtml;

	headerHtml = '';
	innerHtml = '';
	bottamHtml = '';
	html = '';
	progressbarHtml = '';
	data.party.forEach(function(item){
		headerHtml += ` <th style="background:` +  item.color + `">` + item.name + `</th>`;
		innerHtml += `<td style="background:` +  item.color + `;">` +  item.wonlead + `</td>`;
		bottamHtml += `<td>` + item.changes + `</td>`;
		progressbarHtml +=  `<span style="width:` + (item.wonlead/totalWonSeat*100) + `%; background:` +  item.color + `"></span>`; 
	});
	html = `<table cellpadding="0" cellspacing="0"><tr>
					<th>Party</th> ` + headerHtml + `
					</tr>
					<tr>
					<td>Wins + Leads</td> ` + innerHtml + `
					</tr>
					<tr>
					<td>+/- 2014</td> ` + bottamHtml + `
					</tr>
					</table>
					<div class="newelec-tallywidget-right"></div>`;
	document.getElementById('el_party_mah_winseat').innerHTML = `(` + (data.total_seats.seats/2 + 1) + ` Seats to Win)`;
	document.getElementById('el_party_mah_total').innerHTML =  `<span>` + data.total_seats.wonlead + `</span>/ ` + data.total_seats.seats + ` Seats `;
	document.getElementById('el_party_mah_inner').innerHTML = html;
	document.getElementById('el_party_mah_pro_bar').innerHTML = progressbarHtml;
}


