	function getrashifaldata(url, data) {
		var dataid='getrashi-data';	
		var mode ='remove';
		$.ajax({
			type: "POST", url : url, data: data, dataType:'html', success: function(response){	$('#'+dataid).html(response);	}	});	
	}	

	function setCookie(cname,cvalue,exdays) {
		var d = new Date();
		d.setTime(d.getTime() + (exdays*24*60*60*1000));
		var expires = "expires=" + d.toGMTString();
		document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
	}

	function getCookie(cname) {
		var name = cname + "=";
		var decodedCookie = decodeURIComponent(document.cookie);
		var ca = decodedCookie.split(';');
		for(var i = 0; i < ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return "";
	}

	function checkCookie() {
		var user=getCookie("username");
		var pos=getCookie("posrank");
		if (user != "") {
			new Glide('.rashifal-horoscope-sldier2', {	startAt:pos, perView:3,	slidesToScroll: 3, gap:5, slidesToShow: 3,	}).mount();
			var element = document.getElementById(user);	element.classList.add("brdract");
			getrashidata(event, user, pos);	
		} else {
			user = 'aries';
			
			pos = 0;
			new Glide('.rashifal-horoscope-sldier2', {	startAt:pos,perView:3,slidesToScroll: 3,gap:5,slidesToShow: 3,	}).mount();
			var element = document.getElementById(user);	element.classList.add("brdract");
			setCookie("username", user, 30);
		setCookie("posrank", pos, 30);	
	
		}
	}
	
	function getrashidata(evt, rashi, rank) {
		var section = rashi;
		var position = rank;
		var type = 'rhs';	
		var url = "/ajax/home/"+ section +'.html';
		var data = 'section='+section+'&type='+type;
		user = section;
		pos = position;
		setCookie("username", user, 30);
		setCookie("posrank", pos, 30);
		//var element = document.getElementById(section);	
		//element.classList.add("brdract");
		
		getrashifaldata(url, data);
	}
	$(".cursor-pointer").on("click", function(){
		$(".cursor-pointer").removeClass("brdract");
		$(this).addClass("brdract");
	});
