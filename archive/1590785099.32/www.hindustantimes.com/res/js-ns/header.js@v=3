$(document).ready(function(){$(".weather").click(function(){$(this).addClass("active"),$(".blankOverlay").addClass("active")}),$(".login").click(function(){$(this).addClass("active"),$(".blankOverlay").addClass("active")}),$(".moreMenu").click(function(){$(this).toggleClass("active"),$(".blankOverlay").addClass("active")}),$(".anchorSearch").click(function(){$(this).parent().toggleClass("active"),$(".blankOverlay").addClass("active"),$("#newsearchbox_new").focus()}),$(".blankOverlay").click(function(){$(".weather, .login, .moreMenu, .blankOverlay, .anchorSearch").removeClass("active"),$(".anchorSearch").parent().removeClass("active")})}),$(document).on("touchstart touchend",function(a){$(".moreMenu").hasClass("active")&&$(a.target).closest(".dropdown").length}),window.onscroll=function(){stickyNav()};var nav=document.getElementById("nav"),sticky=nav.offsetTop;function stickyNav(){window.pageYOffset>sticky?nav.classList.add("sticky"):nav.classList.remove("sticky")}