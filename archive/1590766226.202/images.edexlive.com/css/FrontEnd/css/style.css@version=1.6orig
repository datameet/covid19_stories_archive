.group-logo{display:none;}

.subscribe_now_txt{ margin:0; padding:0; width:107px; float:left; color:#ff8e5c; font-size:14px; line-height:18px;
 font-family:verdana, sans-serif; }
 
 .subscribe_now_arrow{ margin:0; padding:0; width:30px; float:left; }
 
 .AticleImg{ margin:0; padding:0; width:100%;}

.MobileSearch{ padding:20px 15px 2px 2px; }
.MobileSearch i{ font-size:27px; }
#storyContent p:first-of-type:first-letter{margin:0; padding:0 0 0 3px; float:left; font-size: 75px!important; color:#f4981d; line-height:1;}

}

.navbar-inverse .navbar-nav > .active > a:hover{ color:#000 !important;}
.group-logo {
    float: left;
    margin-top: 40px;
}

@media only screen and (max-width:991px) and (min-width:768px) {
