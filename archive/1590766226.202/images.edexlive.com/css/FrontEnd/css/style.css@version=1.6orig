}
/*for ad image size */
[class^="ad_script"] img { width: auto;}

.article_published_date{
	margin: 15px 0;
    padding: 15px 0 4px;
    float: right;
    width: 100%;
    text-align: right;
}

.article_published_date > p{
    text-align: right;
    font-size: 17px !important;
	padding:0;
}

.article_summary{
	margin-bottom:10px;
	float:left;
	width:100%;
}
.articles_social_icons{
	float: right;
    margin-right: 2%;
}

.articles_social_icons a img{
	width:48px;
	float:left;
	margin-right:10px;
	cursor:pointer;
}

.articles_social_icons a img:hover{
	filter: brightness(130%);
}
.csbuttons-count{
	display:none;
}

.article_tag{
float:left;
width:100%;
}

.social_icon_count{
