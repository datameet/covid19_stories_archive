.social_btn_fb_icon { width: 36px; }
.social_btn_tw_icon { width: 36px; }
.social_btn_insta_icon { width: 40px; margin:0 9px 0 100px; }
.facebook_timeline { width: 54%; margin: 15px auto 20px auto; }
