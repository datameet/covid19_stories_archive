.top_news{ width:92%; float:left; }

.remodal .SectionContainer{  padding:11px 15px 0 15px; }

.StatesHover .MenuItem, .CitiesHover .MenuItem, .AllSectionHover .MenuItem, .StatesHover .MenuItem, .CitiesHover .MenuItem .AllSectionHover .MenuItem { background-color:#f4981d; }

