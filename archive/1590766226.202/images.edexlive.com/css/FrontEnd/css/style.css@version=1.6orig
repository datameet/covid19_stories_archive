    color: #f4981d;
    line-height: 1;
}
.article_fb_count{
    float: left;
    width: 100%;
	border-bottom: 2px solid #0c6ba9;
    margin-bottom: 2px;
	text-align:center;
}

.article_fb_shares{
	font-size: 11px;
    width: 100%;
    float: left;
    text-align: center;
    color: #0c6ba9;
    font-weight:700;
}

article.WidthFloat_L {
    border-bottom: 2px solid #eee;
    padding-bottom: 10px;
}
.scc{min-height: 70px;position: relative;border-top: 1px solid #ddd;padding: 8px 0 11px;border-bottom: 1px solid #ddd;}
.scc .scc-span{font-size: 7px;color: #a3a1a1;margin-bottom: 8px;width: 100%;float: left;text-align: right;}
.scc .scc-div{overflow: hidden;width:100%;align-items: center;display: flex;flex-direction: row;flex-wrap: wrap;justify-content: center;}