                                input.name = inputFields[registrationFields[i]]["name"];
                                input.setAttribute("maxlength", inputFields[registrationFields[i]]["maxlength"]);
                                input.setAttribute("class", inputFields[registrationFields[i]]["class"]);
                                input.setAttribute("onkeypress", inputFields[registrationFields[i]]["onkeypress"]);

