                        submit.setAttribute("data-cbrSubmitButtonStandard", standard);
                        submit.setAttribute("style", "text-align:center");
                        if (lang == "en")
                                submit.innerHTML = "register";
                        if (lang == "hi")
                                submit.innerHTML = "रजिस्टर करें";
                        if (lang == "mr")
                                submit.innerHTML = "नोंदणी करा";

