                                                registerOrResultShowFlag = true;
                                                if (lang == "en")
                                                        heading3.innerText = "Result for " + boardMap[board] + " Board " + standardMap[standard[i]]["name"] + " Results " + year;
                                                if (lang == "hi")
                                                        heading3.innerText = boardMap[board] + " बोर्ड " + standardMap[standard[i]]["name"] + " रिजल्ट " + year;
                                                if (lang == "mr")
                                                        heading3.innerText = boardMap[board] + " बोर्डाच्या " + year + " च्या " + standardMap[standard[i]]["name"] + " निकाल";

                                                list.setAttribute("class", "cbr_active_classbtn");
                                                let element = document.getElementById("cbr-form-" + standard[i] + "-parent");
                                                element.style.display = "block";
                                        }
                                        else {
