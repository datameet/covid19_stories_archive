                function createStandard() {
                        let scholarshipShowFlag = false;
                        let registerOrResultShowFlag = false;

                        let count = 0;
                        let classList = document.getElementById("cbr-class-list");
                        if (standard.length < 2) {
                                scholarshipCancelButton = false;
                                classList.style.display = "none";
                        }
                        for (let i = 0; i < standard.length; i++) {
                                if (standardToResultDeclarationFlagMap[standard[i]] == true && !JSON.parse(boardAndStandardToResultFieldsAndFlagsMapping[board + "|" + standard[i]]).resultAvailableWithVendor)
                                        count = count + 1;
                        }

                        if (count == standard.length) {
                                scholarshipCancelButton = false;
                                scholarshipShowFlag = true;
                        }

                        if (scholarshipShowFlag == true) {
                                document.getElementById("cbr-register-widget-5").innerHTML = "";
                                document.getElementById("cbr-register-widget-1").style.display = "none";
                                createScholarshipDivision();
                                document.getElementById("cbr-register-widget-5").style.display = "block";
                        }
                        else {

                                for (let i = 0; i < standard.length; i++) {
                                        let heading3 = document.getElementById("cbr-register-title");
                                        let list = document.createElement("li");
                                        list.setAttribute("id", "list" + standard[i]);

                                        if (standardToResultDeclarationFlagMap[standard[i]] == false && registerOrResultShowFlag == false) {
                                                registerOrResultShowFlag = true;
                                                if (lang == "en")
                                                        heading3.innerText = "Register to get alert when " + boardMap[board] + " Board Class " + standardMap[standard[i]]["name"] + " Results " + year + " is out";
                                                if (lang == "hi")
                                                        heading3.innerText = boardMap[board] + " बोर्ड कक्षा " + standardMap[standard[i]]["name"] + "  के नतीजों का अलर्ट पाने के लिए रजिस्‍‍‍‍‍‍‍ट्रेशन कराएं ";
                                                if (lang == "mr")
                                                        heading3.innerText = boardMap[board] + " बोर्डाच्या वर्ग " + standardMap[standard[i]]["name"] + " निकालाचे अलर्ट मिळवण्यासाठी नोंदणी करा.";

                                                list.setAttribute("class", "cbr_active_classbtn");
                                                let element = document.getElementById("cbr-form-" + standard[i] + "-parent");
                                                element.style.display = "block";
                                        }
