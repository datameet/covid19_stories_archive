.oi-covid-title {font-size: 12px;font-weight: bold;text-align: center;padding: 2px 5px 0px 5px;color: #000;text-transform: uppercase;background: #fff;}
.oi-covid-title.confirmed {color: #000}
.oi-covid-title.death {color: #ff0505;}
.oi-covid-title.recovered {color: #3fb30a;}
