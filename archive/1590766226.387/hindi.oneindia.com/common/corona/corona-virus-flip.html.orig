.oi-covid-text img {vertical-align: middle;margin-right: 5px;}
.oi-covid-close-cube {position: absolute;right: 0px;top: -15px;width: 20px;height: 15px;background: #757575;z-index: 10000000;cursor: pointer;display: block;border-radius: 3px 3px 0 0;}
