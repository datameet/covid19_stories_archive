.tabs_nav .nav-tabs>li::before, .tabs_nav .nav-tabs>li::after {
  display: none;
}
.tabs_nav .nav-tabs>li.active::before, .tabs_nav .nav-tabs>li.active::after, .tabs_nav .nav-tabs>li:hover::after, .tabs_nav .nav-tabs>li:hover::before {
  display: block;
}
.tabs_nav .nav-tabs>li:first-child::before {
  display: none;
}
.tabs_nav .nav-tabs>li:last-child::after {
  display: none;
}
.tabs_nav .nav-tabs>li:hover > a {
  color: white;
  background-color: #911112;
}
.tabs_nav .nav-tabs>li>a {
  border: 0;
  font-weight: bold;
}
