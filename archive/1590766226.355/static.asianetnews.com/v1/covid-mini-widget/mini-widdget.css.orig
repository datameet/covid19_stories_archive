.tabs_nav .nav li {
    flex: 1;
    background-image: url('images/Divider.png');
    background-repeat: no-repeat;
    width: 100%;
}
.tabs_nav .nav-tabs {
    display: flex;
    justify-content: space-around;
    background: #ffffff;
    border-bottom: 6px solid #ccc;
}
.tabs_nav  .nav-tabs li{
    text-align: center;
}
.tabs_nav  .nav-tabs .nav-link{
      border:0.5px solid #03a0ed !important;
      border-radius: 0px !important;
  }
.tabs_nav .nav-tabs > li a {
    color: #000;
    text-transform: uppercase;
    font-family: Arial, Helvetica, sans-serif;
    display: block;
    line-height: 1;
    padding: .5em 1.5em;
    position: relative;
    text-decoration: none;
    padding: 15px;
  }
.tabs_nav .nav-tabs  li a{
      /* padding: 15px 8px  !important; */
      height: 44px;
      line-height: 22px;
  }
.tabs_nav .nav-tabs > li a:before, li a:after {
    content: '';
    height: 0;
    position: absolute;
    right: 100%;
    top: 0;
    width: 0;
  }
.tabs_nav .nav-tabs > li a:before {
    border-top-color: transparent ;
    border-left-color: transparent ;
  }
.tabs_nav .nav-tabs > li a:after {
    border-right-color: transparent ;
    border-bottom-color: transparent ;
    left: 100%;
    right: auto;
  }
.tabs_nav .nav-tabs >li a:hover:after, li a:focus:before, li a:focus:after {
    border-color: #03a0ed;
  }
.tabs_nav .nav-tabs > li a.active {
    background: #03a0ed;
    cursor: pointer;
    color: #fff;
  }
.tabs_nav .nav-tabs > li a.active:before, li a.active:after {
    border-color: #03a0ed;
    color:#fff;
}
.tabs_nav .nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover {
    color: #fff;
    cursor: default;
    background-color: #911112;
    border-radius: 0;
    border: 0;
}
.tabs_nav .nav-tabs img{
    position: absolute;
    bottom: 0;
    right: auto;
    left: 50px;
    width: 100px;
    z-index: 99;
}
.Right_section .lib-item {
    padding: 0px;
}
.tabs_nav .nav-tabs>li>a {
  margin: 0;
}
.tabs_nav .nav-tabs>li {
    margin-bottom: 0px;
}
.scroll-nav {margin-top: 20px;}
.tabs_nav .nav-tabs>li.last.active {
    border: 0;
    margin: 0;
    background: #911112;
}
.tabs_nav .nav-tabs>li.first.active {
    border: 0;
    margin: 0;
    background: #911112;
    position: relative;
    background-image: url('images/divider-active.jpg');
}
.tabs_nav .nav-tabs>li>a:hover, .nav-tabs>li>a:active, .nav-tabs>li>a:focus {
    border: none;
    background: #911112;
    border-radius: 0px;
    color: #fff;
}
.tabs_nav .nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover {
    color: #fff;
    cursor: default;
    background-color: #911112;
    border-radius: 0;
    border: 0;
}
.tabs_nav .nav-tabs>li>a:hover {
  border: 0;
  background: #911112;
}
.tabs_nav .nav-tabs>li::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 0;
    height: 0;
    z-index: 10;
    background: url('https://static.asianetnews.com/v1/css/malayalam-veteran/images/divider-active.jpg');
    background-size: contain;
    width: 87px;
    height: 65px;
    height: 44px;
    background-position: 0 0;
    background-repeat: no-repeat;
}
.tabs_nav .nav-tabs>li::after {
    content: " ";
    position: absolute;
    right: -26px;
    top: 0;
    width: 0;
    height: 0;
    border-top: 44px solid transparent;
    border-bottom: 0px solid transparent;
    border-left: 26px solid #911112;
    z-index: 1000;
}

