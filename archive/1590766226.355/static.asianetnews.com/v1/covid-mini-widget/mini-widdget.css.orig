  outline: none;
}
.owlCarouselForMultitabStoriesWithCarousel .owl-dots {
  display: block;
  text-align: center;
  margin-top: 5px;
}
.owlCarouselForMultitabStoriesWithCarousel .owl-dots .owl-dot span {
  width: 10px;
  height: 10px;
  margin-right: 5px;
  margin-left: 5px;
  background: #fff;
  border: 1px solid #eee;
  -webkit-box-shadow: 2px 2px 4px 0px rgba(0,0,0,0.27);
  -moz-box-shadow: 2px 2px 4px 0px rgba(0,0,0,0.27);
  box-shadow: 2px 2px 4px 0px rgba(0,0,0,0.27);
  display: block;
  border-radius: 50%;
  outline: none;
}
.owlCarouselForMultitabStoriesWithCarousel .owl-dots .owl-dot.active span,
.owlCarouselForMultitabStoriesWithCarousel .owl-dots .owl-dot:hover span
{
    background-color: #911112;
    border: 2px solid #911112;
    -webkit-box-shadow: 2px 2px 4px 0px rgba(0,0,0,0.27);
    -moz-box-shadow: 2px 2px 4px 0px rgba(0,0,0,0.27);
    box-shadow: 2px 2px 4px 0px rgba(0,0,0,0.27);
}
