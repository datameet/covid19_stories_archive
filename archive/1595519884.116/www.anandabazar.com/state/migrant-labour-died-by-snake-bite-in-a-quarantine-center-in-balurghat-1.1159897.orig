var isRequiredpermissionsOk = true;
FB.api('/me/permissions', function(responsePermission) {
if (responsePermission.data && responsePermission.data != undefined) {
for (i = 0; i < responsePermission.data.length; i++) {
if (responsePermission.data[i].status == 'declined') {
if (responsePermission.data[i].permission == "email") {
isRequiredpermissionsOk = false;
}
}
}
if (!isRequiredpermissionsOk) {
FB.AppEvents.logEvent("EMAIL_PERMISSIONS_DECLINED");
window.dataLayer.push({ 'current_fb_log_status': 'no', 'event': 'pageVariablesLoaded' });
document.location.href = "/login";
} else {
//authorized login
showHideLoginButtons({
socialLoginWrapper: 'none',
