 var el = document.querySelector('#opinion_chart');
el.innerHTML = el.innerHTML.replace(/&nbsp;/g,''); 
  
  

  if(pollid!=undefined){
document.addEventListener("DOMContentLoaded", function(event) {
displayPoll(pollid);
