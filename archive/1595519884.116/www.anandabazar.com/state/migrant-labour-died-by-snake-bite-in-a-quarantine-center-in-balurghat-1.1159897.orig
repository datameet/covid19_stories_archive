<script>
  var getClosest = function (elem, selector) {
		for (; elem && elem !== document; elem = elem.parentNode) {
			if ((elem.dataset['widget'] != "") && (elem.dataset['widget'] != null)) return elem;
		}
		return null;
	};
function showUserRecommendedStories(contanerId){
	userRecommendedStories = JSON.parse(localStorage.getItem("userRecommendedStories"));
	//here first we need to check whether current page is a story page, than remove this story from recommended local storage
	//since this is photo story page hence we are sure here
	tmpArr = location.pathname.split("-");
	currentId = tmpArr[tmpArr.length-1]; //it may be story id or not at all if non story page
	

	if(!userRecommendedStories.hasOwnProperty('lFTUS')){
		userRecommendedStories.lFTUS = [];
	}

	for(i=0;i<userRecommendedStories.lFTUS.length;i++){
		if(userRecommendedStories.lFTUS[i].contentID == currentId){
			//remove this and update local storage and break loop
			userRecommendedStories.lFTUS.splice(i, 1);
			localStorage.setItem("userRecommendedStories", JSON.stringify(userRecommendedStories));
			userRecommendedStories = JSON.parse(localStorage.getItem("userRecommendedStories"));
		}
	}

	recommendedStories = userRecommendedStories.lFTUS;
	if(recommendedStories.length>0){
		if (recommendedStories.length > 5) {
			//only 5 stories are required to be shown
			recommendedStories = recommendedStories.slice(0, 5);
		}
		recommendedHtml = "<div class=\"row\">";
		recommendedHtml += "<div class=\"col-md-6 col-xs-12 abp-more-stories-colblock2 top-10\">";
      //pagetype = "storypage";
		if(recommendedStories.length>0){
			
			//first 3 stories
			$.each(recommendedStories.slice(0, 3), function(index, story) {
				imageSource = story.imageSource.split("_gen")[0];
				imageExtnArr = imageSource.split(".");
				imageExtn = imageExtnArr[imageExtnArr.length-1];
				imageSource = imageSource+"_gen/derivatives/box_140_85/image."+imageExtn;
				recommendedHtml +="<a href=\"" + story.contentUrl + "\" data-widget=\""+pagetype+"-recommendation-stry-"+(index+1)+"\" onclick=\"addMouseDownEventListener(this)\;\">";
				recommendedHtml +="<div class=\"row\">";
				recommendedHtml +="<div class=\"col-md-12 d-flex abp-homepage-140-79-img-side-block\">";
				if(document.domain == "www.anandabazar.com"){
					recommendedHtml +="<div class=\"image\"><img src=\""+"https://images.anandabazar.com"+ imageSource +"\" alt=\""+story.imageAlt+"\" title=\""+story.imageTitle+"\"></div>";
				}else{
					recommendedHtml +="<div class=\"image\"><img src=\""+"https://abpdemo.ttef.in"+ imageSource +"\" alt=\""+story.imageAlt+"\" title=\""+story.imageTitle+"\"></div>";
				}
				recommendedHtml +="<div class=\"text\">";
				recommendedHtml +="<p>"+story.headline+"</p>";
				recommendedHtml +="</div>";
				recommendedHtml +="</div>";
				recommendedHtml +="</div>";
				recommendedHtml +="</a>";
			});
		}

		recommendedHtml += "</div>";
		if(recommendedStories.length>3){
			//last 2 stories
			var counter = 4;
			$.each(recommendedStories.slice(3, recommendedStories.length), function(index, story) {
				recommendedHtml += "<div class=\"col-md-3 col-xs-12 abp-more-stories-colblock2 top-10\">";
				recommendedHtml += 	"<div class=\"row\">";
				recommendedHtml += 		"<div class=\"abp-homepage-244-137-img-down-block\">";
				recommendedHtml += 			"<a href=\"" + story.contentUrl + "\" data-widget=\""+pagetype+"-recommendation-stry-"+(counter++)+"\" onclick=\"addMouseDownEventListener(this)\;\">";
				recommendedHtml += 				"<ul>";
				recommendedHtml += 					"<li class=\"image\">";

				imageSource = story.imageSource.split("_gen")[0];
				imageExtnArr = imageSource.split(".");
				imageExtn = imageExtnArr[imageExtnArr.length-1];
				imageSource = imageSource+"_gen/derivatives/box_240_160/image."+imageExtn;
				

				if(document.domain == "www.anandabazar.com"){
					recommendedHtml += 						"<img src=\""+"https://images.anandabazar.com"+ imageSource +"\" alt=\""+story.imageAlt+"\" title=\""+story.imageTitle+"\" class=\"top-red-border img-fluid\">";
				}else{
					recommendedHtml += 						"<img src=\""+"https://abpdemo.ttef.in"+ imageSource +"\" alt=\""+story.imageAlt+"\" title=\""+story.imageTitle+"\" class=\"top-red-border img-fluid\">";
				}	
				recommendedHtml += 					"</li>";
				recommendedHtml += 					"<li class=\"heading\">";
				recommendedHtml += 						"<div>";
				recommendedHtml += 							"<p>";
				recommendedHtml += 								story.headline;
				recommendedHtml += 							"</p>";
				recommendedHtml += 						"</div>";
				recommendedHtml += 					"</li>";
				recommendedHtml += 				"</ul>";
				recommendedHtml += 			"</a>";
				recommendedHtml += 		"</div>";
				recommendedHtml += 	"</div>";
				recommendedHtml += "</div>";
			});
		}
		recommendedHtml += "</div>";
		$("#" + contanerId).html(recommendedHtml);
		$("#user-recommended-stories-section").css("display", "block");
	}
}
function loadUserRecommendedStories(url, contanerId, isFallback, fallBackUrl) {
	$.ajax({
		url: url,
		success: function(response) {
			userRecommendedStories = JSON.parse(localStorage.getItem("userRecommendedStories"));
			if (!isFallback) {
				userId = localStorage.getItem('userid1');
				recommendedStories = response[userId];
				userRecommendedStories.source="userId";
			}else{
				recommendedStories = response.trending;
				userRecommendedStories.source="trending";
			}
			userRecommendedStories.lFTUS = recommendedStories;
			userRecommendedStories.lFTU = new Date().getTime();
			localStorage.setItem("userRecommendedStories", JSON.stringify(userRecommendedStories));
			showUserRecommendedStories(contanerId);
		},error: function(jqXHR, exception) {
			var msg = '';
			if (jqXHR.status === 0) {
				msg = 'Not connect.\n Verify Network.';
			} else if (jqXHR.status == 404) {
				msg = 'Requested page not found. [404]';
			} else if (jqXHR.status == 500) {
				msg = 'Internal Server Error [500].';
			} else if (exception === 'parsererror') {
				msg = 'Requested JSON parse failed.';
			} else if (exception === 'timeout') {
				msg = 'Time out error.';
			} else if (exception === 'abort') {
				msg = 'Ajax request aborted.';
			} else {
				msg = 'Uncaught Error.\n' + jqXHR.responseText;
			}
			console.log(msg);
			if(!isFallback && fallBackUrl!==""){
				loadUserRecommendedStories(fallBackUrl, contanerId, true, "");
			}
		}
	});
}
window.addEventListener('DOMContentLoaded', (event) => {
	$("a").mousedown(function () {
		addMouseDownEventListener(this);
	});
});
function addMouseDownEventListener(elem){

	if(elem!=undefined && elem!=null){
		var href = elem.href
		if(!href.startsWith("javascript") && !href.startsWith("#")){
			var parent = getClosest(elem, 'data');
			if (parent !== null) {
				var newHref;
				var dataValue = parent.dataset["widget"];
				dataValue = dataValue.toLowerCase();
				if (href.includes('?')) {
					if (href.includes('?ref=')) {
						newHref = href.split("?ref=")[0] + '?' + 'ref=' + dataValue;
					} else {
						newHref = href + '&' + 'ref=' + dataValue;
					}
				} else {
					newHref = href + '?' + 'ref=' + dataValue;
				}
				elem.href = newHref;
			}	
		}else{
			if(!href.startsWith("javascript") && !href.startsWith("#")){
				var newHref;
				if (href.includes('?')) {
					if (href.includes('?ref=')) {
						newHref = href.split("?ref=")[0] + '?' + 'ref=' + elem.dataset['widget'];
					} else {
						newHref = href + '&' + 'ref=' + elem.dataset['widget'];
					}
				} else {
					newHref = href + '?' + 'ref=' + elem.dataset['widget'];
				}
				elem.href = newHref;
			}
		}
	}
}
window.addEventListener('DOMContentLoaded', (event) => {
	userId = localStorage.getItem('userid1');
	userRecommendedStories = localStorage.getItem("userRecommendedStories");
	if(userRecommendedStories==undefined || userRecommendedStories==null || userRecommendedStories.lFTUS==undefined){
		console.log("No story currently");	
		userRecommendedStories = {"lFTU":0, "source" :"", "lFTUS":[]};
		localStorage.setItem("userRecommendedStories", JSON.stringify(userRecommendedStories));
	}
	userRecommendedStories = JSON.parse(localStorage.getItem("userRecommendedStories"));
	var userRecommendationFallback = "https://abp-relatedstory.s3.ap-south-1.amazonaws.com/ABP_Trending_story/";
	if(document.domain == "www.anandabazar.com"){
		userRecommendationFallback+="trending.json";
	}else{
		userRecommendationFallback+="demotrending.json";
	}
	var userRecommendationContainer = "user-recommended-stories";
	var currentDate = new Date();
	var lFTU = new Date(userRecommendedStories.lFTU);
	if(
		(currentDate.getFullYear()> lFTU.getFullYear()) || 
		(currentDate.getFullYear() == lFTU.getFullYear() && currentDate.getMonth() > lFTU.getMonth()) || 
		(currentDate.getFullYear() == lFTU.getFullYear() && currentDate.getMonth() == lFTU.getMonth() && currentDate.getDate()>lFTU.getDate())
	){
		//this is equivalent to day changed or a fresh request 
		if (userId != undefined && userId && userId!=null) {
			//now check userid json is available or not
			$.ajax({
				url: "https://abp-relatedstory.s3.ap-south-1.amazonaws.com/ABP_Recomm/" + userId + ".json",
				type: "HEAD",
				success: function(response) {
					//show from userid json
					loadUserRecommendedStories("https://abp-relatedstory.s3.ap-south-1.amazonaws.com/ABP_Recomm/" + userId + ".json", userRecommendationContainer, false, userRecommendationFallback);
				},error: function(jqXHR, exception) {
					//show from local storage
					loadUserRecommendedStories(userRecommendationFallback, userRecommendationContainer, true, "");
				}
			});
			
		}else{
			loadUserRecommendedStories(userRecommendationFallback, userRecommendationContainer, true, "");
		}
		
	}else{
		//there was no day changed hance show from local storage
		//there is a scenario when earlier stories were loaded from trending but now userid is available hence give it a try
		if (userId != undefined && userId && userId!=null && userRecommendedStories.source=="trending") {
			//now check userid json is available or not
			$.ajax({
				url: "https://abp-relatedstory.s3.ap-south-1.amazonaws.com/ABP_Recomm/" + userId + ".json",
				type: "HEAD",
				success: function(response) {
					//show from userid json
					loadUserRecommendedStories("https://abp-relatedstory.s3.ap-south-1.amazonaws.com/ABP_Recomm/" + userId + ".json", userRecommendationContainer, false, userRecommendationFallback);
				},error: function(jqXHR, exception) {
					//show from local storage
					showUserRecommendedStories(userRecommendationContainer);
				}
			});
		}else{
			showUserRecommendedStories(userRecommendationContainer);	
		}
		
	}
});

</script>  
