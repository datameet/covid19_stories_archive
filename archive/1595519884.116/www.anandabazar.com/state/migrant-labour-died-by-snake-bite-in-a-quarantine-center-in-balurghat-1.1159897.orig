						imageSource  = imageSource+"_gen/derivatives/box_96_72/image."+imageExtn;
					}else{
						imageSource  = "/img/anandabazar-bootstrap/newmdb/image_placeholder_bg.png";
					}
					mostReadHtml+=			"<li class=\"image\"><img src=\""+bgImage+"\" data-src=\""+"https://t.t2online.co.in/unsafe/smart/filters:format(webp)/images.anandabazar.com"+imageSource+"\" class=\"img-fluid rounded-circle lazyload\"  alt=\""+story.imageAlt+"\"></li>";
					mostReadHtml+=			"<li class=\"text d-flex\">";
					mostReadHtml+=				"<span class=\"content\">";
					mostReadHtml+=					story.headline;
					if(story.storyType=="Video"){
						mostReadHtml+=				"<span class=\"align-self-end\"><i class=\"far fa-play-circle\"></i></span>";
					}
					mostReadHtml+=				"</span>";	
					mostReadHtml+=			"</li>";  	
					mostReadHtml+=		"</ul>";	
					mostReadHtml+=	"</a>";
				});
