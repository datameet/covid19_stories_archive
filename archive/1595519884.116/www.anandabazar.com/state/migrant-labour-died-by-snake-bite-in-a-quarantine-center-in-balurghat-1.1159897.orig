function displayPoll(id) {
	$.ajax({
		url: 'https://pollapi.ebela.in/Poll/?qid=' + id + '',
		dataType: 'json',
		type: 'GET',
		success: function(data) {
			var renderHTML = '';
			renderHTML += '<div class="row"><div class="col text">' + data.poll.qText + '</div></div>';
			renderHTML += '<form id="myForm"><div class="row"><div class="col-12 px-0  poll-option-button-wrap"><div class="col-md-12 col-xs-12 d-flex justify-content-center poll-option-block px-0">';
			$.each(data.answer, function(index1, answer) {
				renderHTML += '<div class="form-check"><input type="radio" name="myRadio" class="form-check-input" id="' + answer.answerId + '" value="' + answer.answerId + '"><label class="form-check-label" for="' + answer.answerId + '">' + answer.option + '</lable></div>';
			});
			renderHTML += '</div>';
			renderHTML += '<div class="col-md-12 col-xs-12 d-flex align-items-center justify-content-center poll-submit-block"><a onclick="displayChart(' + data.poll.qId + ');"><div>SUBMIT</div></a></div>  </div></div></form>';
			$("#opinion_div").html(renderHTML);
		},
		error: function() {},
	});
}

function displayChart(id) {
	//  ga('send','event','Ebela Opinion Poll','Opinion Poll Submit Button Clicked','Poll Inserted');
	//ga('send', { 'hitType': 'pageview', 'page': '/', 'title': 'Opinion Poll' });
	var ansId = $('form input[name="myRadio"]:checked').val();
	if ($("form input:radio[name='myRadio']").is(":checked")) {
		showChart(id, ansId);
	} else {
		alert("Please select an Option to participate in the poll");
		return false;
	}
}

function showChart(id, ansId) {
	var loaderHTML = "";
	loaderHTML += '<div class="col-sm-12 abp-new-menu-section-story-container poll-loader" id="loader">';
	loaderHTML += '<div class="loader_div">';
	loaderHTML += '<img src="/img/anandabazar-bootstrap/poll-ajax-loader.gif" alt="" title="" />';
	loaderHTML += '</div></div>';
	jQuery('#opinion_div').html(loaderHTML);
	var renderHTML = '';
	var arr = [];
	$.ajax({
		url: 'https://pollapi.ebela.in/Poll/AbpTotalUserPollingServlet?q_id=' + id + '&ans_id=' + ansId + '',
		dataType: 'json',
		type: 'GET',
		async: true,
		success: function(data) {
			var renderHTML = '';
			renderHTML += '<p class="qtn" style="font-size: 20px; font-weight: bold; margin-bottom: 20px; color: #fec34d; font-family: Noto Serif Bengali; text-align: center;">' + data.poll.qText + '</p>';
			$("#opinoin_ques").html(renderHTML);
			$.each(data.answer, function(indexnew, rownew) {
				var optionText = $('<textarea />').html(rownew.option).text();
				arr.push(optionText);
				arr.push(rownew.votes);
				//      console.log(arr);
			});
			$(".opinion_col").css('display', 'none');
			google.charts.load('current', { 'packages': ['corechart'] });
			if (arr.length == 4) {
				//    console.log(arr.length);
				google.charts.setOnLoadCallback(drawChart);
			} else if (arr.length == 6) {
				google.charts.setOnLoadCallback(drawChart1);
			} else if (arr.length == 8) {
				google.charts.setOnLoadCallback(drawChart2);
			}
			//arr.length == 4
			function drawChart() {
				var total = parseInt(arr[1]) + parseInt(arr[3]);
				var data = google.visualization.arrayToDataTable([
					['option', 'Vote', { role: 'style' }, { role: 'annotation' }],
					[arr[0], parseInt(arr[1]), 'stroke-color: #703593; stroke-width: 2; fill-color:gold;', parseInt((parseInt(arr[1]) * 100) / total) + '%'],
					[arr[2], parseInt(arr[3]), 'stroke-color: #703593; stroke-width: 2; fill-color: #b87333;', parseInt((parseInt(arr[3]) * 100) / total) + '%'],
				]);
				var options = {
					title: 'আপনার মত',
					titleTextStyle: { color: 'black', fontSize: 14, fontName: 'Arial' },
					hAxis: {
						textStyle: {
							color: 'black',
							fontName: 'Arial Black',
							fontSize: 11
						}
					},
					vAxis: { textPosition: 'none' },
					chartArea: {
						width: 290,
						height: 170
					},
					legend: { position: 'none' },
					tooltip: {
						trigger: "none"
					}
				};
				var chart = new google.visualization.ColumnChart(document.getElementById('opinion_chart'));
				chart.draw(data, options);
			}
			// arr.length == 6
			function drawChart1() {
				var total = parseInt(arr[1]) + parseInt(arr[3]) + parseInt(arr[5]);
				var data = google.visualization.arrayToDataTable([
					['option', 'Vote', { role: 'style' }, { role: 'annotation' }],
					[arr[0], parseInt(arr[1]), 'stroke-color: #fff; stroke-width: 0; fill-color:#056FBF;', parseInt((parseInt(arr[1]) * 100) / total) + '%'],
					[arr[2], parseInt(arr[3]), 'stroke-color:#fff ; stroke-width: 2; fill-color:#023F6D;', parseInt((parseInt(arr[3]) * 100) / total) + '%'],
					[arr[4], parseInt(arr[5]), 'stroke-color: #fff; stroke-width: 2; fill-color:#022946;', parseInt((parseInt(arr[5]) * 100) / total) + '%']
				]);
				var options = {
					title: 'আপনার মত',
					titleTextStyle: { color: 'black', fontSize: 14, fontName: 'Arial' },
					hAxis: {
						textStyle: {
							color: 'black',
							fontName: 'Arial Black',
							fontSize: 12
						}
					},
					vAxis: { textPosition: 'none' },
					chartArea: {
						width: 220,
						height: 140
					},
					legend: { position: 'none' },
					tooltip: {
						trigger: "none"
					}
				};
				var chart = new google.visualization.ColumnChart(document.getElementById('opinion_chart'));
				chart.draw(data, options);
			}
			// arr.length == 8
			function drawChart2() {
				var total = parseInt(arr[1]) + parseInt(arr[3]) + parseInt(arr[5]) + parseInt(arr[7]);
				var data = google.visualization.arrayToDataTable([
					['option', 'Vote', { role: 'style' }, { role: 'annotation' }],
					[arr[0], parseInt(arr[1]), 'stroke-color: #703593; stroke-width: 2; fill-color:gold;', parseInt((parseInt(arr[1]) * 100) / total) + '%'],
					[arr[2], parseInt(arr[3]), 'stroke-color: #703593; stroke-width: 2; fill-color:#b87333;', parseInt((parseInt(arr[3]) * 100) / total) + '%'],
					[arr[4], parseInt(arr[5]), 'stroke-color: #703593; stroke-width: 2; fill-color:silver;', parseInt((parseInt(arr[5]) * 100) / total) + '%']
					[arr[6], parseInt(arr[7]), 'stroke-color: #703593; stroke-width: 2; fill-color:#e5e4e2;', parseInt((parseInt(arr[7]) * 100) / total) + '%']
				]);
				var options = {
					title: 'আপনার মত',
					titleTextStyle: { color: 'black', fontSize: 14, fontName: 'Arial' },
					hAxis: {
						textStyle: {
							color: 'black',
							fontName: 'Arial Black',
							fontSize: 12
						}
					},
					vAxis: { textPosition: 'none' },
					chartArea: {
						width: 210,
						height: 140
					},
					legend: { position: 'none' },
					tooltip: {
						trigger: "none"
					}
				};
				var chart = new google.visualization.ColumnChart(document.getElementById('opinion_chart'));
				chart.draw(data, options);
			}
			$("#opinion_chart").css('display', 'block');
			jQuery('#loader').hide();
		},
	});
} 
  }
</script>

                                      
                                  
            	                        
