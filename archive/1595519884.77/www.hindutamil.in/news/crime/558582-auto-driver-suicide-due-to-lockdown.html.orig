		var cnt = data.engagement.reaction_count; 
		if(cnt==0 || cnt=='0'){}else{
			if(cnt > 1000){
				cntx = (cnt/1000);
				cnt1 = cntx.toFixed(1) +' k';
			}else{
				cnt1 = cnt;
			}
			$('.fbCount').html(cnt1);
			$('.fbCount').show();
		} 
	},
	error: function(data){  console.log(data);  }
});
</script>

 