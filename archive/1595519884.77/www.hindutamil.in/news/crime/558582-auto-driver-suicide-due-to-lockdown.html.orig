	$(window).ready(function() { 
		
		$("img.lazy").lazyload({ effect : "fadeIn" });
		
		//$('.comment-box').html('1'); 
		
		$(window).on("scroll", function () {
			if ($(this).scrollTop() !==0) {
				$('#toTop').fadeIn();
			}
			else {
				$('#toTop').fadeOut();
			}
		});
		
		$('#toTop').on("click", function () {
			$("html, body").animate( {
				scrollTop: 0
			}, 600);
			return false;
		}); 
		
		/* comment section */
		var lenCmt = parseInt($('.comment-box').length); 
		if (lenCmt > 0) {
			var artArr = '';
			var tmpArr = [];
			$('.comment-box').each(function(idx, ele){ 
				var x = $(this).attr('data-id');
				artArr += x +',';
				tmpArr[idx] = x;
			}); 
			if (artArr!=''){
				$.ajax({
					url: 'https://www.hindutamil.in/ajax/common.php?act=cmt_cnt&do=cmt_cnt',
					type: "POST",
					data: {aid:artArr},
					success: function(response) {
						if(response.trim()=='empty'){ 
							$('.comment-box').each(function(idx, ele){
								$(this).css('padding', '0px')
								$(this).html('')
							});
						}else{
							var obj = jQuery.parseJSON(response);
							//console.log(obj);
							$('.comment-box').each(function(idx, ele){
								if(obj[tmpArr[idx]]=='undefined' || obj[tmpArr[idx]]=='' || obj[tmpArr[idx]]==null){
									$(this).css('padding', '0px')
									$(this).html('')
								}else{
									$(this).html(obj[tmpArr[idx]])
								} 
							}); 
						}
					}            
				}); 
			}
		} 
		/* comment section ends here */
		
		$('.newsletter_submit').click(function(){
			var EmailId = $('#ns_email').val(); 
			if(EmailId!='' && validateEmail(EmailId)){
				$(".newsletter_response").html('Please wait...');
				$.ajax({
					url:'https://www.hindutamil.in/ajax/common.php?act=fed&do=nwsletter',
					type:'POST',
					data:$("#newletterform").serialize(),
					success:function(result){
						$(".newsletter_response").html(result);
						$('#ns_email').val(''); 
					}
				});
			}else{
				$(".newsletter_response").html('<span class="text-danger">Please enter Valid Email Address</span>');
			}
			return false;
		});
		
	}); 

	function validateEmail(sEmail) {
		var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
		if (filter.test(sEmail)) {
			return true;
		}
		else {
			return false;
		}
	}

