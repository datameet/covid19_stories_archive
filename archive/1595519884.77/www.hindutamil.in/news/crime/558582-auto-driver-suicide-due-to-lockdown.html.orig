			data:{ cookieName:'termAccept', cookieValue:1, cookieDays:365 },
			success:function(result){ }
		}); 
	}

	function follow_author(author_id, user_id){
		$.ajax({
			url: 'https://www.hindutamil.in/ajax/common.php?act=follow_Author&do=follow_Author',
			type: "POST",
			data: {user:user_id, author:author_id},
			success: function(response) {
				if(response.trim()=='success'){
					$('#authErr').html('<span class="text-success">&nbsp</span>');
					$('#flobtn'+author_id).html('Following'); 
				}else{
					$('#authErr').html(response);
				}
			}            
		}); 
	}

	function alertLogin(){
		$('#authErr').html('<a href="https://www.hindutamil.in/login" class="text-danger">Please login to follow</a>');
	}

