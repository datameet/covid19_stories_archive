function graphfbpop(overrideLink,overrideTitle,overrideDescription,overrideImage,width,height){if(overrideImage.length===0){overrideImage=Drupal.settings.itg_flag.settings.default_image}else overrideImage=overrideImage;overrideTitle=decodeURIComponent((overrideTitle+'').replace(/\+/g,'%20'));overrideDescription=decodeURIComponent((overrideDescription+'').replace(/\+/g,'%20'));FB.ui({method:'share_open_graph',action_type:'og.shares',action_properties:JSON.stringify({object:{'og:url':overrideLink,'og:title':overrideTitle,'og:description':overrideDescription,'og:image':overrideImage,'og:image:width':width,'og:image:height':height}})},function(response){console.log('Graph share')})}
function fbpopSearch(overrideLink,overrideTitle,overrideDescription,overrideImage,base_url,node_id){var str_title=jQuery('<div/>').html(overrideTitle).text();console.log(str_title,'--str');loadScript('//connect.facebook.net/en_US/all.js',function(){if(overrideImage.length===0){overrideImage=Drupal.settings.itg_flag.settings.default_image}else overrideImage=overrideImage;var app=Drupal.settings.itg_flag.settings.fb_app;FB.init({appId:app,xfbml:true,version:'v2.10'});FB.ui({method:'share_open_graph',action_type:'og.shares',action_properties:JSON.stringify({object:{'og:url':overrideLink,'og:title':str_title,'og:description':overrideDescription,'og:image':overrideImage}})},function(response){var front_uid=Drupal.settings.itg_flag.settings.uid;if(typeof node_id!=='undefined')jQuery.ajax({url:base_url+'/fb-share-callback/'+node_id+'/'+front_uid,type:'POST',dataType:'JSON'});if(front_uid>0)jQuery.ajax({url:base_url+'/itg-global-fb-point/'+node_id+'/facebook_share',type:'POST',dataType:'JSON'})})})}
function snappost_percentage(like,dislike,type){var total_like_dislike=like+dislike,result=0;if(type=='like'){result=Math.round((like*100)/total_like_dislike)}else result=Math.round((dislike*100)/total_like_dislike);return result}
function getCookiesFun(c_name){if(document.cookie.length>0){c_start=document.cookie.indexOf(c_name+"=");if(c_start!=-1){c_start=c_start+c_name.length+1;c_end=document.cookie.indexOf(";",c_start);if(c_end==-1)c_end=document.cookie.length;return unescape(document.cookie.substring(c_start,c_end))}};return""}
