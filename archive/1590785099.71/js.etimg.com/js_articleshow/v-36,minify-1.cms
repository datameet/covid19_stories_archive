            if (curSc <= prevSc  && !$('.articlelist_container:hover').length) {
                // Scroll up
                    _objBtmBar.removeClass('showBtmBar');
                if($(window).scrollTop() > topNavPos && !_objTopBar.hasClass('fixNavBar'))    {
                    _objTopBar.addClass('fixNavBar');
                }
            }
            else {
                // 'Scroll down'
