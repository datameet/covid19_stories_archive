(function(b){b.fn.newslider=function(f,e){var a=b(this);a.find("li").first().addClass("imgactive");b(e).click(function(){a.find(".imgactive").removeClass("imgactive").addClass("oldActive");if(a.find(".oldActive").is(":first-child")){a.find("li").last().addClass("imgactive")}else{a.find(".oldActive").prev().addClass("imgactive")}a.find(".oldActive").removeClass("oldActive")});b(f).click(function(){a.find(".imgactive").removeClass("imgactive").addClass("oldActive");if(a.find(".oldActive").is(":last-child")){a.find("li").first().addClass("imgactive")
}else{a.find(".oldActive").next().addClass("imgactive")}a.find(".oldActive").removeClass("oldActive")})};b.fn.rkslider=function(B,a,r,u,C,F,v,E,w){var w=w||false;var z=b(this);var x=z.find("ul li").size();if(F!=""){var s=Math.ceil(x/F)}else{var s=x}var y=x*C;var t="-"+(y-C*F);var A;var D=1;z.find("ul").css("width",y);b(B).click(function(c){c.preventDefault();if(w){var e=D==parseInt(x)?0:D;var d=z.find("ul li:eq("+(e)+")").height();z.height(d)}if(D<s){z.find("ul").animate({left:"-="+u},500);D++}else{if(D==s){z.find("ul").animate({left:"0"},1000);
D=1}}if(v=="yes"){b(E).find("ul li.current").removeClass("current");b(E).find("ul li:eq("+(D-1)+")").addClass("current")}});b(a).click(function(c){c.preventDefault();if(w){var e=D==1?x:D-1;var d=z.find("ul li:eq("+(e-1)+")").height();z.height(d)}if(D>1){z.find("ul").animate({left:"+="+u},500);D--}else{if(D==1){z.find("ul").animate({left:t},1000);D=s}}if(v=="yes"){b(E).find("ul li.current").removeClass("current");b(E).find("ul li:eq("+(D-1)+")").addClass("current")}});if(r=="true"){A=setInterval(function(){if(D<s){z.find("ul").animate({left:"-="+u},500);
D++}else{if(D==s){z.find("ul").animate({left:"0"},1000);D=1}}if(v=="yes"){b(E).find("ul li.current").removeClass("current");b(E).find("ul li:eq("+(D-1)+")").addClass("current")}},7000);b(this).bind("mouseenter",function(){clearInterval(A);A=null});b(this).bind("mouseleave",function(){A=setInterval(function(){if(D<s){z.find("ul").animate({left:"-="+u},500);D++}else{if(D==s){z.find("ul").animate({left:"0"},1000);D=1}}if(v=="yes"){b(E).find("ul li.current").removeClass("current");b(E).find("ul li:eq("+(D-1)+")").addClass("current")
}},7000)})}if(v=="yes"){b(E).find("ul li:first").addClass("current")}if(v=="yes"){b(E).find("ul li a").click(function(c){c.preventDefault();b(E).find("ul li.current").removeClass("current");var d=b(E).find("ul li").index(b(this).parent());z.find("ul").animate({left:"-"+(u*d)},500);b(this).parent().addClass("current");D=(d+1);if(w){var e=z.find("ul li:eq("+(d)+")").height();z.height(e)}})}};b.fn.bottomSlider=function(o,m,k,l){btmSliderHolderwidth=b(this).width();scrollwidth=b(this).find("li").width();
btmSliderScrollwidth=scrollwidth+22;btmSliderCurrentLastLi=k;var p=b(this);btmSliderTotalLI=b("#outerTabContainer").find("li").length;var j=btmSliderTotalLI*btmSliderScrollwidth;var a="-"+(j-btmSliderHolderwidth*l);var n;btmSliderSlideClick=1;p.find("ul").css("width",j);b(o).click(function(c){var d=parseInt(b("#firstLi").css("left"));if(d>-screenWidth&&d<=0){if(btmSliderSlideClick<6){if(btmSliderSlideClick<btmSliderTotalLI){p.find("li").animate({left:"-="+btmSliderScrollwidth},100);btmSliderCurrentLastLi++;
btmSliderSlideClick++}else{if(btmSliderSlideClick>=btmSliderTotalLI){p.find("li").animate({left:"0"},100);btmSliderSlideClick=1}}}else{b(o).hide("fast");b(m).attr("style","")}if(btmSliderSlideClick>1){b(o).attr("style","");b(m).attr("style","")}if(btmSliderSlideClick==6){b(o).hide("fast");b(m).attr("style","")}}});b(m).click(function(c){var d=parseInt(b("#firstLi").css("left"));if(d>=-screenWidth&&d<0){if(btmSliderSlideClick>1){p.find("li").animate({left:"+="+btmSliderScrollwidth},100);btmSliderCurrentLastLi--;
btmSliderSlideClick--}else{if(btmSliderSlideClick==1){p.find("li").animate({left:a},100);btmSliderSlideClick=btmSliderTotalLI}else{b(m).hide();b(o).attr("style","")}}if(btmSliderCurrentLastLi==k||btmSliderSlideClick==1){b(m).hide();b(o).attr("style","")}else{b(m).attr("style","");b(o).attr("style","")}}})}}(jQuery));