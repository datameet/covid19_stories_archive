.contact-info{ float:left; width:220px; margin:20px 10px 20px 20px;}
.contact-info2{ float:left; width:320px; margin:20px 10px 20px 20px;}
.contact-info h1,.contact-info2 h1{ float:left; width:100%; font-size:22px; color:#FFF; font-weight:normal; margin:0px; padding:0px;}
.contact-info h2,.contact-info2 h2{ float:left; width:100%; font-size:16px; color:#666; font-weight:normal; margin:10px 0px 0px 0px; padding:0px;}
