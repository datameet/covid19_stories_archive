        <script type="text/javascript">
    $(window).scroll(function () {
        var y = $(this).scrollTop();

        if (y > 800) {
            $('.scrolltop').fadeIn();
        } else {
            $('.scrolltop').fadeOut();
        }
    });

    $(document).ready(function () {

        $(".card-text:empty").hide();

        $(".scrolltop").click(function () {
            $("html, body").animate({ scrollTop: 0 }, "slow");
            return false;
        });



        $('#city').on('paste', function (event) {
            exp = /^[a-zA-Z]*$/;
            if (!exp.test(event.originalEvent.clipboardData.getData('Text'))) {
                return false;
            }
        });

        $('#city').autocomplete({
            source: function (request, response) {
                var text = $("#city").val();
                $.ajax({
                    url: "/Weather/GetCities?name=" + text,
                    type: "GET",

                    success: function (data) {

                        if (data != null && data.length > 0) {
                            response($.map(data, function (item) {
                                return { label: item.Name + ", " + item.Country, value: item.Name + ", " + item.Country, lat: item.Lat, lon: item.Lon };
                            }))
                        }

                    }
                })
            },
            delay: 0,
            select: function (e, i) {
                getWeather(i.item.lat, i.item.lon);
                $("#city").hide();
            },

            minLength: 3
        });

        document.getElementById("todayDate").innerHTML = GetTodayDate();

        getWeather("30.680223899999998", "76.83479489999999");
    });

    $(window).scroll(function () {
        var sticky = $('#myheader'),
            scroll = $(window).scrollTop();

        if (scroll >= 150) sticky.addClass('sticky');
        else sticky.removeClass('sticky');
    });

    function ToggleShareIcon(obj) {
