#wrap {
	margin: 0 auto;
	height: 32px !important;
}

.inner {
	margin: 0 auto;
    max-width:97%;
}

.relative {
	padding:0;
}

.right {
	float: right;
}

.left {
	float: left;
}

.footerMenuR1{
	width: 100%;
	float: left;
	background: #626262;
	border-radius: 5px;
	height: 43px;
	line-height: 43px;
	margin-top: 14px;
	color:#ffffff;
	text-align:center;
	font-family:Arial,Helvetica,sans-serif;
}

.footerMenuR1>span:last-child{
	display:none;
}

/* HEADER */
#wrap > header {
    background-color: #E7A02F !important;
    float: left;
    padding: 0;
    margin: 0px;
    width: 100%;
	height:32px !important;

}
.logo {
	display: inline-block;
    font-size: 0;
    padding: 3px 0;
    float: left;
}
#navigation {
	position: relative;
	float: left;
    margin: 0;
    padding: 0;
}

#menu-toggle {
	display: none;
	float: left;
}

.src_mnu
{
	padding:0;
	margin:0;
	float:right;
}
.src_mnu ul
{
	display:block;
	padding:0;
	margin:0;
	float:left;
}
.src_mnu li
{
	display:inline-block;
	padding:4px 3px;
	margin:0;
	color:white;
   vertical-align: middle;
}
.src_mnu li a
{
    font-size: 12px;
    color: white;
    font-weight: bold;
	font-family:arial;
	
}



/* HEADER > MENU */
#main-menu-right
{
	float: right;
	font-size: 0;
	margin: 10px 0;

}
/* HEADER > MENU */
#main-menu {
    float: left;
    font-size: 0;
    margin: 0;
    padding: 0;
}

#main-menu > li {
	float: left;
	display: inline-block;
	margin-right: 6px;
	padding: 10px 0;
}

#main-menu > li.parent {
	background-size: 7px 7px;
	background-repeat: no-repeat;
	background-position: left center;
	cursor:pointer;
}

#main-menu > li.parent-hds {
	background-size: 1px 1px;
	background-repeat: no-repeat;
	background-position: left center;
}


#main-menu > li.parent > a {
	padding-left: 12px;
    
}

#main-menu > li > a {
	color: #eee;
	font-size: 12px;
	line-height: 14px;
	font-weight:bold;
	float: left;
	text-decoration:none;
}

#main-menu > li:hover > a,
#main-menu > li.current-menu-item > a {
	color: #23dbdb;
}

/* HEADER > MENU > DROPDOWN */
#main-menu li {
	position: relative;
}

ul.sub-menu { /* level 2 */
	display: none;
	left: 0px;
	top: 20px;
	padding-top: 10px;
	position: absolute;
	width: 150px;
	z-index: 9999;
	list-style:none;
	cursor:pointer;
}

ul.sub-menu ul.sub-menu { /* level 3+ */
	margin-top: -1px;
	padding-top: 0;
	left: 149px;
	top: 0px;
}

ul.sub-menu > li > a {
	background-color: #4997C4;
	border: 1px solid #428AB8;
	border-top: none;
	color: #ffffff;
	display: block;
	font-size: 13px;
	line-height: 13px;
	padding: 10px 12px;
		cursor:pointer;
}

ul.sub-menu > li > a:hover {
	background-color: #2a2a2a; 
	color: #fff;
}

ul.sub-menu > li:first-child {
	border-top: 3px solid #23dbdb;
}

ul.sub-menu ul.sub-menu > li:first-child {
	border-top: 1px solid #444;
}

ul.sub-menu > li:last-child > a {
	border-radius: 0 0 2px 2px;
}

ul.sub-menu > li > a.parent {
	background-image: url(../images/arrow.png);
	background-size: 5px 9px;
	background-repeat: no-repeat;
	background-position: 95% center;
}

#main-menu li:hover > ul.sub-menu {
	display: block; /* show the submenu */
}




