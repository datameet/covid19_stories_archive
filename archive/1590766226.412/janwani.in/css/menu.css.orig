@media only screen and (max-device-width: 991px) {

	.flipbook-viewport .container,
	.flipbook-viewport .shadow,
	.flipbook-viewport .shadow>.page-wrapper,
	.flipbook-viewport .shadow>.page-wrapper>div,
	.flipbook-viewport .shadow>.page-wrapper>div>div,
	.flipbook-viewport .shadow>.page-wrapper>div>div>.shadowimage,
	.flipbook-viewport .shadow>.page-wrapper>div>div>.shadowimage>canvas,
	.flipbook-viewport .shadow>.page-wrapper>div>div>.shadowimage>img

	{
		width: 100% !important;
	}



	.menuBtn1{
		display: block !important;
		cursor: pointer;
    	margin-left: 10px !important;
	}

	#main-menu{
		display: none;
	}

.sidenav {
		opacity: 0;
		display: block !important;
		height: 100%;
		width: 0;
		position: fixed;
		z-index: 10000;
		top: 0;
		left: 0;
		background-color: #254990;
		overflow-x: hidden;
		transition: 0.5s;
		padding-top: 60px;
		box-shadow: 2px 0px 5px #333;
	}
	
	.sidenav>li>a {
		padding: 7px 8px 7px 22px;
   		font-size: 16px;
		text-decoration: none;
		color: #CBCBCB;
		display: block;
		transition: 0.3s;
		line-height: 1.5;
	}
	
	.sidenav>li>a:hover, .offcanvas>li>a:focus{
		color: #f1f1f1;
	}
	
	.sidenav .closebtn {
		position: absolute;
	    top: 10px;
	    right: 10px;
	    font-size: 40px;
	    background: #1d3e80;
	    color: #CBCBCB;
	    padding: 0 8px;
	    line-height: 1;
	    height: 40px;
	}
	
	.rSubMenu{
		display: block;
		margin-bottom: 10px;
		margin-left: 35px;
		list-style: none;
		padding-left: 15px;
	}
	
	.rSubMenu>li>a{
		color: #f1f1f1;
		font-size: 16px;
		line-height: 1.5;
	}
	
	.rSubMenu>li{
		margin: 10px 0;
	}
}





