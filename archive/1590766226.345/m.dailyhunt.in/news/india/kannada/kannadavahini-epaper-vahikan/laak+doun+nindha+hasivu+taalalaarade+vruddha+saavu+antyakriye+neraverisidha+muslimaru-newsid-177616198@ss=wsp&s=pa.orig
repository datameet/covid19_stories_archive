function getCookie(cname) {
 var name = cname + "=";
 var ca = document.cookie.split(';');
 for(var i=0; i<ca.length; i++){
    var c = ca[i];
    while (c.charAt(0)==' ') c = c.substring(1);
    if (c.indexOf(name) != -1)
     return c.substring(name.length,c.length);
   } return "";
}

function createCookie(e, s, etime) {
 if(etime == undefined){
 etime = 2629746000;
 }
   var t = new Date;
   t.setTime(t.getTime() + etime);
   var a = "; expires=" + t.toGMTString();
   document.cookie = e + "=" + s + a + ";path=/";
}
