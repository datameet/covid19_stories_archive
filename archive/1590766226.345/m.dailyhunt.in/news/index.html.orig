      var OSName="dux";
      if (navigator.appVersion.indexOf("Win")!=-1) OSName="dw";
      if (navigator.appVersion.indexOf("Mac")!=-1) OSName="dm";
      if (/\bCrOS\b/.test(navigator.userAgent)) OSName="da";
      // if (navigator.appVersion.indexOf("X11")!=-1) OSName="dux";
      // if (navigator.appVersion.indexOf("Linux")!=-1) OSName="dux";
      return OSName;
    }

