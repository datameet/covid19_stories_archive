                        }
                        responce && responce.code && responce.code==200 && responce.data && cb(responce.data)
                    }
                }
            };

            xmlhttp.open("GET", "ajax/wapAjax.php?"+'criUrl='+url, true);
            xmlhttp.send();
        }

        var updateTicker = function(data){
            var rows = data && data.type=='TickerNode' && data.tickers && data.tickers.length>0 && data.tickers;
            var tickerRows = [];
            for(i=0;i<rows.length;i++){
                var data = '';
                try {
                    data = JSON.parse(rows[i]['jsonFormatData']);
                } catch(e) {
                    console.log('Not Json Data')
                }

                data && (data[0]['id']=rows[i]['id'],updateTemplate(data[0]))
            }
        }

        function updateTemplate(data) {
            if ("undefined" == typeof data || 0 === Object.keys(data).length) return 0;

            var obj = {};
            var id = data.id;
            var team1 = data && data.team1;
            var team2 = data && data.team2;
            var t1NoInn = team1 && team1.innings && team1.innings.noOfInngs;
            var t2NoInn = team2 && team2.innings && team2.innings.noOfInngs;

            var t1Cs = team1 && team1.innings && team1.innings.currentScore;
            var t1Cw = team1 && team1.innings && team1.innings.currentWicket && team1.innings.currentWicket;

            var t1Ps = team1 && team1.innings && team1.innings.pastScore;
            var t1Pw = team1 && team1.innings && team1.innings.pastWicket && team1.innings.pastWicket;

            var t2Cs = team2 && team2.innings && team2.innings.currentScore;
            var t2Cw = team2 && team2.innings && team2.innings.currentWicket && team2.innings.currentWicket;

            var t2Ps = team2 && team2.innings && team2.innings.pastScore;
            var t2Pw = team2 && team2.innings && team2.innings.pastWicket && team2.innings.pastWicket;

            obj['macthNo_'+id] = data.desc;
            obj['time_'+id] = data.matchTime;
            obj['crr_'+id] = data.runRate;
            obj['status_'+id] = data.status;
            obj['team1currentOver_'+id] = team1 && team1.innings && team1.innings.currentOver;
            obj['team2currentOver_'+id] = team2 && team2.innings && team2.innings.currentOver;
            obj['t2Cs_'+id] = (t2NoInn>0) && ((t2Cw && t2Cs) ? t2Cs+'<em>/</em>'+t2Cw : (t2Cs?t2Cs:0)+'<em>/</em>'+(t2Cw?t2Cw:0));
            obj['t1Cs_'+id] = (t1NoInn>0) && ((t1Cw && t1Cs) ? t1Cs+'<em>/</em>'+t1Cw : (t1Cs?t1Cs:0)+'<em>/</em>'+(t1Cw?t1Cw:0));
            obj['t1Ps_'+id] = (t1NoInn>0) && ((t1Pw && t1Ps) ? t1Cs+'<em>/</em>'+t1Pw : (t1Ps?t1Ps:0)+'<em>/</em>'+(t1Pw?t1Pw:0));
            obj['t2Ps_'+id] = (t2NoInn>0) && ((t2Pw && t2Ps) ? t2Cs+'<em>/</em>'+t2Pw : (t2Ps?t2Ps:0)+'<em>/</em>'+(t2Pw?t2Pw:0));
//            team2currentOver
            //console.log(t2NoInn,t1NoInn,t1Cw,t2Cw)
//            console.log(data)
//            console.log(obj)
            for(key in obj){
                (obj[key] && obj[key].trim()!=='') && document.getElementById(key) && (document.getElementById(key).innerHTML=obj[key])
            }
        }

        //tickerUpdateUrl && makeRequest(tickerUpdateUrl,updateTicker);
        setInterval(function(){
            tickerUpdateUrl && makeRequest(tickerUpdateUrl,updateTicker);
        }, (tickerRefTime*100));

    })()
