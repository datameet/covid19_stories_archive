var weatherResponse = null;
var selectCityOptions = window.innerWidth > 576 ? document.getElementById('selectCity') : document.getElementById('selectCityMob');
var temperature = window.innerWidth > 576 ? document.getElementById('temperature') : document.getElementById('temperatureMob');
var weatherIcon = window.innerWidth > 576 ? document.getElementById('weatherIcon') : document.getElementById('weatherIconMob');
var currentTempC = '';
var cities = [];

function getWeatherDetails() {
  if(weatherResponse == null) {
    fetchWeatherDetails();
  } else {
    updateWeatherDetails();
  }
}
function fetchWeatherDetails () {
    fetch("")
    .then((response) => {
      return response.json();
    })
    .then((res) => {
      weatherResponse = res;
      cities = weatherResponse.map((item) => { return Object.keys(item) });
      cities.forEach(function(item, index){
        var option = document.createElement("option");
        option.id = item;
        option.text = item;
        selectCityOptions.add(option);
      })
      updateWeatherDetails();
    })
    .catch((err)=> {
      console.log('weather err',  err)
    })
  }

  function updateWeatherDetails(){
    var staticIconPath = 'https://static.asianetnews.com/v1/weather-widget/icons/PNGs_64x64/';
    for(var item in weatherResponse) {
      if(Object.keys(weatherResponse[item])[0] === selectCityOptions.value) {
        currentTempC = weatherResponse[item][selectCityOptions.value].data.current_condition[0].temp_C;
        //weatherIcon.src = weatherResponse[item][selectCityOptions.value].data.current_condition[0].weatherIconUrl[0].value;
        var iconUrlDefault = weatherResponse[item][selectCityOptions.value].data.current_condition[0].weatherIconUrl[0].value;
        weatherIcon.src = staticIconPath + iconUrlDefault.match(/\w+(?=\.png)\.png/)[0];

      }
    }
    temperature.innerHTML = currentTempC + '&#176' + ' ' + 'C';
  }
</script> </div></div><div class='top-1'><div class='article-breadcrumbs'><div class="breadcrum-box"><div class="container"><div class="row"><div class="col-md-12"><ul class="breadcrumb" itemscope itemtype="http://schema.org/BreadcrumbList"><li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem" itemid="https://bangla.asianetnews.com"> <a title="Home" href="/" itemprop="item"><span itemprop="name">Bangla News</span></a><meta itemprop="position" content="1"> </li>  <li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem" itemid="https://bangla.asianetnews.com/coronavirus"><a title="Coronavirus" href="/coronavirus" itemprop="item"><span itemprop="name">Coronavirus</span></a><meta itemprop="position" content="2"> </li>  <li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem" itemid="https://bangla.asianetnews.com/coronavirus-westbengal"> <a title="Coronavirus West Bengal" href="/coronavirus-westbengal" itemprop="item"><span itemprop="name">Coronavirus West Bengal</span></a><meta itemprop="position" content="3"> </li></ul></div></div></div></div></div></div><div class="container" style="margin-bottom: 20px;"></div><div class="template-content"><div class="container"><section class="category-top-1 art-layer-2"><div class="left-side"><div class='top-2'><div class='article-header'><style>
