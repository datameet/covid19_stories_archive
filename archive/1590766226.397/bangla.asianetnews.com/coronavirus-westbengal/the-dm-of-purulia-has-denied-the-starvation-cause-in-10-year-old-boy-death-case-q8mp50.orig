            //bind img src on load
            $("#menuContent0 .owl-carousel").find("img").each(function(key, value){
              if($(value).attr("src") === "") {
                  $(value).attr("src", $(value).attr("data-src"));
              }
            });
          }, 
          error: function(error){
            console.log("error text:  ", error)
          }
        });
      }
