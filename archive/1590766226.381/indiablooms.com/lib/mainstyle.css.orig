/*general election*/

.general-election{ width: 745px;}
.general-election h2{ text-align: center;
    margin: 0;
    background-color: #500142;
    color: #fff;
    padding: 10px;
    box-sizing: border-box;
    display: inline-block;
    width: 745px;
    vertical-align: top;
    text-transform: uppercase;
    font-weight: normal;}
.general-election-part{
	display: inline-block;
    width: 745px;
    vertical-align: top;
    background-color: #035682;
    padding: 8px;
    box-sizing: border-box;
}
.general-election-part .ul-listing{padding-left: 0; margin: 0; list-style-type: none; margin-bottom: 8px; box-sizing: border-box;}
.general-election-part .ul-listing li{ color: #fff; position: relative; padding-left: 15px; box-sizing: border-box; 
	line-height: 20px; margin-bottom:0;}
.general-election-part .ul-listing li a{color: #fff; text-decoration: none; font-family:'Helvetica 85 Heavy'}
.general-election-part .ul-listing li:after{ background: url(../images/arrow.png); width: 6px; height: 10px; position: absolute;
left: 0; top: 3px; content: '' }
.general-election-left{ width: 20%; float: left;
border-left: #9cbbda 1px solid;
border-top: #9cbbda 1px solid;
border-right: #9cbbda 1px solid;
border-bottom: #9cbbda 1px solid; }
.general-election-left-head,.general-election-left-body{
	display: inline-block;
    width: 100%;
    vertical-align: top;
}
.general-election-left-head span{ width: 50%; float: left; background-color: #0a2a48; padding:25px 5px; 
	color: #fff; font-size: 13px; text-align: center; min-height: 68px; box-sizing: border-box;
}
.general-election-left-head span.red-bg{ background-color: #fe0002; }
.general-election-left-body ul{ list-style-type: none; padding-left: 0; margin: 0 }
.general-election-left-body ul li{ display: inline-block; width: 100%; vertical-align: top; border-bottom: #9cbbda 1px solid }
.general-election-left-body ul li span{ width: 50%; float: left; color: #fff; text-align: center; 
	padding:17px 5px; background-color: #fff; color: #353535;  box-sizing: border-box;
	font-family: 'Helvetica 85 Heavy';     font-size: 15px;
}
.general-election-left-body ul li span.span-title{ background-color: #0c3e6a; color: #fff; }
.general-election-left-body ul li span.light-blue-bg{background-color: #c1dcf1;}

.general-election-right{ width: 79.7%; float: left; }
.election-2019-all .general-election-right{ width: 100%; float: left; }
.general-election-right table{
	border-collapse: collapse;
	border: #ebe1f0 2px solid;
	width: 100%;
	margin-bottom: 5px;
}
.general-election-right table tr td{ border:#ebe1f0 2px solid; background-color: #fff; vertical-align: top;     font-family: 'Helvetica 85 Heavy';}
.general-election-right table tr td span.title{ background-color: #a0a0a0; display: block; color: #fff; 
text-align: center; padding:5px 10px; border-radius: 2px; box-sizing: border-box;}
.general-election-right table tr td.color-1 span.title{background-color: #fd9a01;}
.general-election-right table tr td.color-2 span.title{background-color: #03bffe;}
.general-election-right table tr td.color-3 span.title{background-color: #0102fd;}
.general-election-right table tr td.color-4 span.title{background-color: #9a01f1;}
.general-election-right table tr td.color-5 span.title{background-color: #a0a0a0;}
.general-election-right table tr td span.subtitle{ 
text-align: center;
text-transform: uppercase;
padding: 2px 8px; box-sizing: border-box;
display: inline-block;
width: 100%;
vertical-align: top;  min-height: 36px;     font-size: 18px;     color: #58595B;}
.general-election-right table tr td span.subtitle .total-seat{ position: static; font-size: 100%;display: block; 
	color: #b74339; font-size: 11px; }
.general-election-right table tr td span.subtitle .declared{ position: static; font-size: 100%;display: block;  
	color: #404847;  font-size: 11px;  }
.general-election-right table tr td span.subtitle em{ font-style: normal; }
.election-result-source{ color: #fff; font-size: 14px; margin: 0; text-align:right}
.general-election-right table tr td span.subtitle{    padding-top: 11px;}
.general-election-right table tr td:nth-child(1) span.subtitle{ padding-top: 4px;}

.general-election-assembly span{ width: 100%; float: left; background-color: #C13E66; padding:4px 5px; 
	color: #fff; font-size: 25px; text-align: left; min-height: 67px; box-sizing: border-box; line-height:30px;
}
.assemelec {background-color: #C13E66;}
.general-election-right table tr td.color-assemelec span.title{background-color: #C13E66; color:#fff; border-color:#C13E66}
.assemelec table tr td span.title{ background-color: #C13E66; display: block; color: #fff; 
text-align: center; padding:5px 10px; border-radius: 2px; box-sizing: border-box;}

