/* Start Tab Style */


ul.tabs {
	margin: 0px;
	padding: 0px;
	float: left;
	list-style: none;
	/*border-bottom: 1px solid #ddd;
	border-left: 1px solid #ddd;
	width: 1010px;*/
	height:27px;
}
ul.tabs li {
	float: left;
	padding: 0px 10px;
	height: 24px;
	line-height: 24px;
	border:1px solid #808285;
	border-bottom:none;
	/*margin-bottom: -1px;*/
	background:#DCDDDE;
	text-align:center;
	position: relative;
	margin-top:3px;
}
ul.tabs li a {
	text-decoration: none;
	display: block;
	font-family:'Helvetica 55 Roman';font-weight:normal;font-size:18px;
	color:#0F5E90;
	padding: 0px 10px;
	outline: none;
	border-left:none;
}
ul.tabs li a:hover {
	/*background:#ffffff;
	border:none;*/
	color:#000000;
}
html ul.tabs li.active {
	background:#ffffff;
	border:1px solid #808285;
	border-bottom:none;
	height: 27px;
	line-height: 27px;
	margin-top:-3px;
	padding-top:3px;	
}

html ul.tabs li.active a:hover {
	background:#ffffff;
	padding:0 10px;
	display:block;
	/*border:1px solid #808285;
	border-bottom:none;
	height: 27px;
	line-height: 27px;
	margin-top:-3px;
	padding-top:3px;*/	
}

