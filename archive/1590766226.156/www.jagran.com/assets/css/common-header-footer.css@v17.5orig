.menuLinks .container2 section li h3 a:hover{color: #f00;}
.bottomLinks.menuLinks .container section2 li a:hover{ color:#ffd800;}
/*blink css*/
.blinkLive{padding-left:18px; position:relative;}
	.blinkLive:before{ position: absolute; top:5px; left:2px; border: 2px solid #f9b0b3; content:""; width:8px; height:8px; background:red; border-radius: 50%; text-decoration: blink;-webkit-animation-name: blinker;-webkit-animation-duration: 0.6s;-webkit-animation-iteration-count:infinite;-webkit-animation-timing-function:ease-in-out;-webkit-animation-direction: alternate;}
	@-webkit-keyframes blinker { from {opacity: 1.0;} to {opacity: 0.0;}}

/*Wiki Template css start*/	
.wikiBox {margin-bottom:10px; clear: both; border-radius: 10px; box-shadow: 1px 1px 18px 0px #d6d6d6; overflow:hidden;}
.wikiDetails li{color:#898989; font-size:17px; width:100%; float:left;}
.wikiDetails li:nth-child(even){ background-color:#fafafc;}
.wikiDetails li .box{border-bottom:1px solid #e5e5e5; padding:10px 10px 10px 20px; margin: 0 10px; overflow:hidden;}
.wikiDetails li:first-child .box{ padding:25px 10px 25px 20px;}
.wikiDetails li:last-child .box{ border-bottom:0px;}
.wikiDetails li figure{ width:125px; height:125px; border-radius:50%; overflow:hidden;}
.wikiDetails h2{ width:100%; font-size:28px; color:#000; border-bottom:0px; margin-bottom:5px;}
.wikiDetails .subheding, .wikiDetails .socialFollow{ width:100%; float:left; display:block;}
.wikiDetails .socialFollow a{ display:inline-block; margin:20px 5px 0 0;}
.wikiDetails .socialFollow a:hover{ opacity:0.8;}
.wikiDetails .socialFollow i{width:30px; height:30px; background-image:url('https://www.jagranimages.com/images/wiki-icons.png'); background-repeat:no-repeat;}
.wikiDetails .socialFollow .fb{background-position:0px 0px;}
.wikiDetails .socialFollow .tw{background-position:-37px 0px;}
.wikiDetails .socialFollow .insta{background-position:-76px 0px;}
.wikiDetails .socialFollow .tumblr{background-position:-114px 0px;}
.wikiDetails li .fl{ width:30%; position:relative;}
.wikiDetails li:first-child .fl:after{ content:'';}
.wikiDetails li .fl:after{ content:':'; position:absolute; right:14px; top:0px; font-size:20px;}
.wikiDetails li .fr{ width:70%; color:#000;} 
.wiki-tabbx {text-align: center;  margin: 50px 0px 0px;} 
.wikiTabs {display:inline-block; margin-bottom:10px; clear: both; border-radius: 30px; box-shadow: 1px 1px 10px 0px #d6d6d6; padding: 7px;}
.wikiTabs li{display: inline-block;line-height: 27px;}
.wikiTabs li a {font-size: 18px;font-weight: bold;padding:5px 20px;display: inline-block;font-family: "Baloo",cursive; font-weight:normal;}
.wikiTabs li.active a {background: #dc131b;color: #fff;border-radius: 30px; position: relative; padding:5px 20px 5px 8px;}
.wikiTabs li.active a:before {content: "";width: 22px;height: 22px;border-radius: 50%;display: inline-block;background: #fff;vertical-align: middle;margin-right: 5px;}
.wikiTabs li.active a:after {content: "";border: solid #dc131b;border-width: 0 2px 2px 0;display: inline-block;padding: 3px 3px 8px 3px;transform: rotate(45deg);-webkit-transform: rotate(45deg);position: absolute; 
left: 15px; top: 10px;}
.wikiCompo li{ border-top:0px;}
.wikiCompo .protxt{ padding-top:0px;}
.adSpace { min-height:233px; }
/*Wiki Template css end*/	

