/*heading links*/

.heading_link{clear: left; font-weight: bold;line-height: 29px;display: inline-block;margin-bottom: -38px;margin-top: 6px;border-radius: 5px 5px 0px 0px;position: relative;z-index: 9999;}
.heading_link.fl{margin-left: 20%;}
.heading_link li{position: relative; float:left; padding-right:10px; margin-right:30px; }
.heading_link li:last-child{ margin-right:5px;}
.heading_link li:before { position: absolute; right: -1px;top: 10px;content: "";width: 0px;height: 0px;border-top: 5px solid transparent;border-bottom: 5px solid transparent;border-left: 5px solid #F00;}

/*-Inner Pages-*/
.bigVideo{ margin-bottom:30px; float:left; width:100%;}
.commonList .photoIcon i { background-position: -43px -340px; width: 16px; height: 16px; display: block; border: 1px solid #FFF; border-radius: 6px; }

.commonList .photoIcon {  margin: 0px; padding: 7px; background: #F00 none repeat scroll 0% 0%; display: none; position: absolute; bottom: 0px; left: 10px;}
.photoGallery .commonList.size3 li { margin: 0px .5%; margin-bottom: 15px;} 
.newK ,.newK1 { z-index: 99!important; top: 98px!important; }

/*jagran video*/
.MainHd.videoHd{ margin-bottom:0;}
.videoHd ul{ margin:4px 20px 0px 0px; display:block; }
.videoHd li, .tagHd li{ margin-left: 10px; position: relative; float: left; font-size: 15px; line-height: 28px; font-weight: bold; padding: 3px 20px 3px; }
.videoHd li.active, .tagHd li.active { background: #ffd800; }
.videoHd .active:before, .tagHd .active:before { position: absolute; left: -7px; top: 0px; content:""; width: 0; height: 0; border-top: 34px solid transparent;border-bottom: -2px solid transparent; border-right:7px solid #ffd800; }
.videoHd .active:after, .tagHd .active:after { position: absolute; right: -7px; top: 0px; content:""; width: 0; height: 0; border-top: -2px solid transparent;border-bottom: 34px solid transparent; border-left:7px solid #ffd800; }
.tagHd li:last-child{ margin-right:10px;}
.videoBox { clear: left; background: #000; overflow: hidden; margin-bottom:50px;}
.videoBox li { padding: 20px 2% 0 2%; width: 20.9%; position: relative; float: left; border-left: 1px solid #333333; }
.videoBox li figure { position: relative; height: 137px; } .videoBox li figure img { height: 100%; }
.videoBox li:first-child, .videoBox li:nth-child(5){border-left: 0px; }
.videoBox li:last-child { padding-right: 0px; }
.videoBox p { padding:10px 0px 5px; color: #bcb7b7; position: relative; font-weight: 600; height:88px; overflow:hidden; }
.videoBox i.videoIcon { margin-top: 3px;width: 25px;height: 25px;float: left;position: absolute;margin-right: 10px;background-color: #F00;right: -6px;top: -35px;}
.videoBox i.videoIcon:before { position: absolute; top: 50%;margin-top: -8px;left: 50%;margin-left: -3px;content: "";border-top: 8px solid transparent;border-bottom: 8px solid transparent;
border-left: 10px solid #FFF;border-radius: 3px;}
/*lic*/
.ticker-container, .container2{ max-width:980px; padding:0 5px; clear: left; margin:0px auto;}
.menuNavigation { position:absolute!important;}
.container2 section li a:hover{ color:#f00;}
.menuLinks .container2 section {width:20%; float: left; clear: none;}
.menuLinks .container2 section h3, .menuLinks .container2 section li h3 a{font-size: 13px; font-weight: bold;}
.menuLinks .container2 section h3:hover a {border-bottom: 1px solid #da271c;}
.menuLinks .container2 section ul{margin:4px 0 0; font-size:16px; list-style-type:none; min-height:178px;}
