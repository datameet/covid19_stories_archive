.firstImg .h3 {padding:4px 0 0; height:83px; overflow:hidden;}
.firstImg figure img { display: inline-block; width:100%; margin-top:-10%;}
.newsBox ul li span,.newsBox ul li span a {color:#b30f18;font-size:10px; text-transform: uppercase; line-height:13px;}
.MainHd { border-bottom: 1px solid #f1f1f1; float: left; width: 100%; margin-bottom: 15px; position:relative; }
.newsBox small { margin-left:5px; padding-left:5px; color: #585757; font-weight: 400; display:inline-block; text-transform:capitalize; font-size: 11px;}
.videoListing .commonList.size3 li {margin: 0px 0.5% 15px;}
.commonList { width: 100%; float: left; }
.commonList li { width:18.4%; float:left; background: #fff; margin-left:2%;border-bottom: 2px solid #f3f3f3;padding-bottom: 10px; box-shadow:0 0 7px #a99e9e1c; border-radius: 10px;}
.commonList.size3 li { width:32.32%; margin-left: 1.5%; }
.commonList.size2 li { width:49%; margin:17px 0 0 0;}
.commonList.size2 li:nth-child(1), .commonList.size2 li:nth-child(2) { margin: 0px;}
.commonList.size2 li figure:nth-child(1), .commonList.size2 li figure:nth-child(2) { height:119px;}
.commonList.size2 li:nth-child(odd) { float: left;}
.commonList.size2 li:nth-child(even) { float: right;}
.commonList.size1 li{ width:100%; margin:0 0 20px; padding-bottom:5px;}
.commonList.size1 li figure{ height:160px;}
.commonList.size1 li .h3{ height:inherit; margin-top:5px;}
.commonList li:first-child { margin-left: 0px; }
.commonList li .h3 { padding:5px 9px 5px; height: 90px; overflow: hidden; display: block; clear: left; }
.commonList li span {  padding: 10px 9px 0px; display: block; height: 14px; overflow: hidden; }
.commonList li figure { width: 100%; height:110px; overflow:hidden; border-radius: 10px 10px 0 0px;}
.commonList li figure img, .newsBox li figure img, .topicList li figure img{ width: 100%; color: #f5f5f5; font-size: 10px;}
.commonList li:nth-child(5n+1) { margin-left: 0px; }
.firstImg li i.videoIcon{bottom:50px!important;}
.newsBox li i.videoIcon {margin-top: 3px;width: 25px;height: 25px;float: left;position: absolute;margin-right: 10px;background-color:#ff0000; right:-5px;bottom:14px;}
.newsBox i.videoIcon:before {position: absolute;top: 50%;margin-top:-8px;left: 50%;margin-left: -3px;content: "";border-top: 8px solid transparent;border-bottom: 8px solid transparent;border-left: 10px solid #FFF;border-radius: 3px;}
.newsBox li figure, .topicList li figure{ position:relative;background: #f5f5f5 url(https://www.jagranimages.com/images/jagranlogo-noimage.png) no-repeat center; background-size: 145px;}
.newsBox .commonList li figure{background-size: 78px;}
.newsBox li figure::before { position: absolute; top: 0; left: -75%; z-index: 2; display: block; content: ''; width: 50%; height: 100%; background: -webkit-linear-gradient(left, rgba(255,255,255,0) 0%, rgba(255,255,255,.3) 100%); background: linear-gradient(to right, rgba(255,255,255,0) 0%, rgba(255,255,255,.3) 100%); -webkit-transform: skewX(-25deg); transform: skewX(-25deg); }
.newsBox li figure:hover::before {-webkit-animation: shine .75s; animation: shine .75s;}
@-webkit-keyframes shine { 100% {left: 125%; }}
@keyframes shine {100% { left: 125%; }}
.aarPadhe{ display:inline-block; padding:4px 0px 2px; margin-top:4px; line-height:23px; font-weight: bold; float: right; font-size: 12px; border-radius: 15px; }
.aarPadhe span{ color: #000; }

