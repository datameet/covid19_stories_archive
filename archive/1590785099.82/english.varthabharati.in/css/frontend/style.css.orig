    position: relative;
    margin-bottom: 0px;
    display: inline;
}

/*** Navigation ***/
.nbs-flexisel-nav-left,
.nbs-flexisel-nav-right {
    width: 44px;
    height: 60px;
    position: absolute;
    cursor: pointer;
    z-index: 100;
    display: none;
}

.nbs-flexisel-nav-left {
    left: -7em;
    background: url(../images/left.png) 0px 0px no-repeat;
    display: none;
}

.nbs-flexisel-nav-right {
    right: -7em;
    background: url(../images/right.png) 0px 0px no-repeat;
    display: none;
}

