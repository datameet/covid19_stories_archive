function isObjEmpty(o) {for(var prop in o) {if(o.hasOwnProperty(prop))return false;}return JSON.stringify(o) === JSON.stringify({});};
function formatBytes(a,b){if(0==a)return"0 Bytes";var c=1024,d=b||2,e=["Bytes","KB","MB","GB","TB","PB","EB","ZB","YB"],f=Math.floor(Math.log(a)/Math.log(c));return parseFloat((a/Math.pow(c,f)).toFixed(d))+" "+e[f]};
function formatTitle(n, c, r = 1) { return ( n.length < c ) ? n : n.substring(0, r) + '...'; };
function _showSnackbar(msg) {if (_$("#snackbar")) {_$("#snackbar").setHTML("").setHTML(msg).addCls("show");setTimeout(function(){ _$("#snackbar").remCls("show"); }, 3000);}}
function _getKeyCode(e){if (window.event) return window.event.keyCode; else if (e) return e.which; else return null;};
function _createElement(){var elm;try{elm = document.createElement( "<" + arguments[0] + "></" + arguments[0] + ">" );} catch(e){elm = document.createElement( arguments[0] );}for(a = 0; a < arguments[1].length; a++)elm.setAttribute(arguments[1][a][0], arguments[1][a][1]);return elm;};
function insertAfter(newNode, referenceNode) {referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);};
function __getData(o,a){var iF = -1;var d = null;for(i = 0; i < o.attributes.length; i++){if (o.attributes[i].name != null && o.attributes[i].name == ("data-" + a)) {iF = i;d = o.attributes[i].value;break;}}return d;}
function _attachAnEvent(c, ev, fn) {if (c) {if(c.addEventListener){c.addEventListener(ev, fn, true);}else if(c.attachEvent){c.attachEvent("on" + ev, fn);}}};
function _detachAnEvent(c, ev, fn) {if (c) {if (c.removeEventListener){c.removeEventListener(ev, fn, true);}else if (c.releaseEvents){c.releaseEvents("on" + ev, fn);}}};
function _getHash(){return window.location.hash;}
function _setHash(a){window.location.hash = window.location.hash == '' ? '#' + a : a;}
function _setCookie(c_name,value,expiredays){var exdate=new Date();exdate.setDate(exdate.getDate()+expiredays);document.cookie=c_name+ "=" +escape(value)+	((expiredays==null) ? "" : ";expires="+exdate.toGMTString());};
function _getCookie(c_name){if (document.cookie.length>0){c_start=document.cookie.indexOf(c_name + "=");if (c_start!=-1){c_start=c_start + c_name.length+1;c_end=document.cookie.indexOf(";",c_start);if (c_end==-1) c_end=document.cookie.length;return unescape(document.cookie.substring(c_start,c_end));}}return "";};
function _loader(m){var dLB = _$('.loaderTrack');switch(m){case 'show':if(dLB.selected.length <= 0){lb = _createElement("div", [["class", "loaderTrack"]]);lt = _createElement("div", [["class", "loader"]]);lb.appendChild(lt);document.body.appendChild(lb);_moveLoader();} else console.log("Loader is already shown");break;case 'hide': if(dLB.selected.length > 0){document.body.removeChild(dLB.selected[0]);clearTimeout(st);} else console.log("No loader to hide");break;}};
function _moveLoader(){var dLT = _$('.loader');if(dLT.selected.length > 0){var wid = window.innerWidth || document.body.clientWidth;dLT = dLT.selected[0];var lft = dLT.style.left != '' ? dLT.style.left : (wid * 0.3 * -1);lft = (parseInt( lft ) + 1) >= wid ? (wid * 0.3 * -1) : (parseInt( lft ) + 1);dLT.style.left = lft + 'px';st = setTimeout("_moveLoader()", 0.75);}};
function _allowNumeric(e){var keyp;keyp = _getKeyCode(e);if(keyp >= 48 && keyp <= 57 || keyp >= 96 && keyp <= 105)return true;else if(keyp == null)return true;else{if(keyp == 8 || keyp == 0 || keyp == 46 || keyp == 9 || keyp == 190)return true;else return false;}};
function _allowAlpha(e){var keyp;keyp = _getKeyCode(e);if(keyp >= 60 && keyp <= 95 )return true;else if(keyp == null)return true;else{if(keyp == 8 || keyp == 0 || keyp == 46 || keyp == 32 || keyp == 9 || keyp == 190) return true; else return false;}};
function _genRandomStr(l = 10){var chrs = 'abcdefghijklmnopqrstuvwxyz0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ'.split('');var chrLen = (chrs.length - 1);var strRnd = '';for (i = 0; i < l; i++)strRnd += chrs[parseInt(Math.floor(Math.random() * chrLen) + 1)];return strRnd;};
function _disableView(){var dV = _$('#viewDiable');if(dV.selected.length === 0){dV = _createElement("div", [["id", "viewDiable"],["style", "position:fixed;width:100%;height:100%;top:0;left:0;background-color: rgba(0, 0, 0, 0.3);"]]);document.body.appendChild(dV);}else{document.body.removeChild(dV.selected[0]);_disableView();}};
function _enableView(){var dV = _$('#viewDiable');if(dV.selected.length > 0){document.body.removeChild(dV.selected[0]);}};