var isInViewport=function(elem=null){if(elem!=null){var bounding=elem.getBoundingClientRect();return(bounding.top>=0&&bounding.left>=0&&bounding.bottom<=(window.innerHeight||document.documentElement.clientHeight)&&bounding.right<=(window.innerWidth||document.documentElement.clientWidth))}};var loadImage=function(){var image=document.querySelectorAll('.lazyimg');for(var img=0;img<=image.length;img++){if(isInViewport(image[img])){var cls='';if(image[img].getAttribute('data-class')!=''&&image[img].getAttribute('data-class')!=null)
cls='class="'+image[img].getAttribute('data-class')+'"';var stl='';if(image[img].getAttribute('data-style')!=''&&image[img].getAttribute('data-style')!=null)
stl='style="'+image[img].getAttribute('data-style')+'"';if(image[img].getAttribute('data-webp')!=''&&image[img].getAttribute('data-webp')!=null)
image[img].innerHTML='<img src="'+image[img].getAttribute('data-image')+'" alt="'+image[img].getAttribute('data-alt')+'" '+cls+' '+stl+'>'}}};window.addEventListener('scroll',function(event){loadImage()},!1);window.addEventListener('load',function(event){loadImage()},!1)