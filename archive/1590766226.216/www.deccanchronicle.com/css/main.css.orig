/* Cookies Responsive */
@media only screen and (min-width:768px) and (max-width:1024px) { .cookie-popup-box .padding-inner {padding:40px 10px 12px !important;} .cookie-popup-box p {font-size:13px !important;} .cookie-popup-box .close-button  {padding:7px 25px 7px !important;font-size:14px !important;} .cookie-popup-box .popup-button {margin:8px 0px 0px 6px !important;} }
@media screen and (max-width:767px) { .cookie-close  {right:10px;} .cookie-popup-box .padding-inner {padding:40px 20px 15px !important;} .cookie-popup-box p {margin-top:0px !important;font-size:14px !important;width: 100% !important;} .cookie-popup-box .popup-button {margin:12px 0px 0px 0px !important;width:100% !important;text-align: left !important; } .cookie-popup-box .close-button {padding:7px 40px 7px !important;border-radius:8px !important;} }
