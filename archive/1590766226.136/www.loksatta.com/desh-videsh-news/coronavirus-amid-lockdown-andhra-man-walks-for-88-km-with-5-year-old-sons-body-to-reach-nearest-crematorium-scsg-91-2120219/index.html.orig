jQuery(document).ready(function(){
	jQuery('.lsbanner-close-trigger').click(function(){
		createCookied('topappbannerclick','topappbannervalue', 60000*60 );
		jQuery( '#topbannapp' ).hide();
	});
	
	function createCookied(cookieName,cookieValue,TimeExpire){
		var date = new Date();
		date.setTime( date.getTime() + TimeExpire );
		document.cookie = cookieName + "=" + cookieValue + "; expires=" + date.toGMTString();
	}	
	
	function getaccessCookie(cookieName){
		var name = cookieName + "=";
		var allCookieArray = document.cookie.split(';');
		for(var i=0; i<allCookieArray.length; i++){
			var temp = allCookieArray[i].trim();
			if (temp.indexOf(name)==0)
				return temp.substring(name.length,temp.length);
		}
		return "";
	}

	function show_lsappbanner() {
		if ( jQuery( "#topbannapp" ).length > 0 ) {
			try {
				var lsappbancookieval = getaccessCookie( 'topappbannerclick' );
				if ( 'topappbannervalue' != lsappbancookieval || null === lsappbancookieval || 'undefined' === lsappbancookieval ) {
					jQuery('#topbannapp').show();
				} else {
					jQuery('#topbannapp').hide();
				}
			} catch (err) {

			}
		}
	}
	setTimeout( function() { show_lsappbanner() }, 2000 );
});

