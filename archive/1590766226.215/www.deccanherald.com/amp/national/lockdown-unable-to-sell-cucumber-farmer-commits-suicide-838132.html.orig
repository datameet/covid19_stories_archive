amp-accordion section[expanded] .side-nav-list__items--accordion-cta:after {
    -ms-transform-origin: center;
    transform-origin: center;
}
amp-accordion section:not([expanded]) .side-nav-list__items--accordion-cta:before {
    -ms-transform: rotate(180deg);
    transform: rotate(180deg);
}
amp-accordion section:not([expanded]) .side-nav-list__items--accordion-cta:after {
    -ms-transform-origin: center;
    transform-origin: center;
}
