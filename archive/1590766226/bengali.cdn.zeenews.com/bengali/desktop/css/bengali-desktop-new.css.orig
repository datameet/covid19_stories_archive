.wrapper{max-width:1366px;margin:0 auto;overflow-x:hidden;}
.page-header{position:fixed;top:0;width:100%;left:0;right:0;box-sizing:border-box;-webkit-transition:top .6s;-moz-transition:top .6s;transition:top .6s;margin:auto;z-index:9999}
.page-header.off-canvas{top:-85px}
.page-header.fixed{top:0;z-index:9999}
.top-ad-show{position:fixed;top:-100px}
.top-ad-show{display:none !important;}
.top-ad{padding:5px 0}
.margin-auto{margin:auto}
.margin-bt10px{margin-bottom:10px}
.margin-bt20px{margin-bottom:20px!important}
.margin-bt30px{margin-bottom:30px}
.margin-bt40px{margin-bottom:40px}
.margin-bt50px{margin-bottom:50px}
.margin-tp30px{margin-top:30px}
.lang-section{background:#ededed;text-align:center;padding:5px 0}
.lang-section a{color:#535353;font-size:14px}
.lang-section span{color:#535353;font-size:9px;vertical-align:middle}
.lang-section span:after{content:"|";padding:0 8px}
.red-navigation{background:#e21727;height:55px;position:relative}
.nav-f-block{background:#000;width:10%;padding:5px 0px 5px 28px;float:left;height:55px;}
.nav-f-block img{margin-left:5px;}
#nav-icon{width:22px;height:15px;position:relative;display:inline-table;vertical-align:middle;-webkit-transform:rotate(0deg);-moz-transform:rotate(0deg);-o-transform:rotate(0deg);transform:rotate(0deg);-webkit-transition:.5s ease-in-out;-moz-transition:.5s ease-in-out;-o-transition:.5s ease-in-out;transition:.5s ease-in-out;cursor:pointer}
#nav-icon span{display:block;position:absolute;height:1px;width:100%;background:#fff;opacity:1;left:0;-webkit-transform:rotate(0deg);-moz-transform:rotate(0deg);-o-transform:rotate(0deg);transform:rotate(0deg);-webkit-transition:.25s ease-in-out;-moz-transition:.25s ease-in-out;-o-transition:.25s ease-in-out;transition:.25s ease-in-out}
#nav-icon span:nth-child(1){top:0}
#nav-icon span:nth-child(2){top:7px}
#nav-icon span:nth-child(3){top:14px}
#nav-icon.open span:nth-child(1){top:7px;-webkit-transform:rotate(135deg);-moz-transform:rotate(135deg);-o-transform:rotate(135deg);transform:rotate(135deg)}
#nav-icon.open span:nth-child(2){opacity:0;left:-60px}
#nav-icon.open span:nth-child(3){top:7px;-webkit-transform:rotate(-135deg);-moz-transform:rotate(-135deg);-o-transform:rotate(-135deg);transform:rotate(-135deg)}
.navbar-fixed-top{position:fixed;top:0;width:100%;z-index:9999;margin:auto;max-width:1366px}
.navbar-fixed-bottom-zee{position:fixed;z-index:9999;top:30px;width:100%;bottom:inherit;margin:auto;max-width:1366px}
.nav-s-block{width:70%;float:left;padding:0 10px;height:55px;position:relative}
.nav-s-block a{color:#fff;font-size:22px;padding:16px 12px;display:block;position:relative;text-decoration:none;height:55px;-webkit-transition:background .2s linear;-moz-transition:background .2s linear;-ms-transition:background .2s linear;-o-transition:background .2s linear;transition:background .2s linear}
.nav-s-block a:hover{background:#f13746;border-bottom:5px solid #c30615}
.nav-s-block a.active-link{background:#f13746;border-bottom:5px solid #c30615}
.nav-t-block{width:20%;float:left}
.social-block{margin:0 10px;height:55px;width:100%;text-align:center}
.social-block li{list-style:none;padding:16px 10px;display:inline-table}
.social-block li span i{padding-left:5px;padding-right:5px}
.social-block li span,.social-block li a{color:#FFF;font-size:18px}
.social-hover{position:relative}
.social-hover:hover{background:#f13746;cursor:pointer}
.social-show{width:160px;position:absolute;top:55px;right:0;display:none;background:#FFF;-webkit-box-shadow:0 1px 2px 0 rgba(0,0,0,0.75);-moz-box-shadow:0 1px 2px 0 rgba(0,0,0,0.75);box-shadow:0 1px 2px 0 rgba(0,0,0,0.75);z-index:99;cursor:text;text-align:left;padding:10px}
.mega-menu{display:none;background:#000;padding:10px;width:100%;}
.mega-menu-scroll{position:fixed;max-width:1366px;z-index:9998;top:55px}
.d-none ul{margin:0;padding:0}
.d-none ul li{list-style:none;float:left}
.nav-block{display:none}
.dropbtn{color:#fff;text-align:center;text-decoration:none}
.dropdown-content{display:none;position:absolute;background-color:#f9f9f9;min-width:160px;box-shadow:0 4px 8px 0 rgba(0,0,0,0.2);z-index:1}
.dropdown-content a{display:block;float:none;color:#000}
.dropdown-content a:hover{background-color:#f1f1f1}
.drop:hover .dropdown-content{display:block}
.nav-border{border-right:1px solid #202020}
.header-story{margin:0;padding:0 0 0 20px;max-width:390px;border-left:1px solid #434343}
.header-story li{list-style:none;padding:24px 0}
.header-story li a{color:#FFF;font-size:16px;display:block}
.top-footer{padding:20px 15px;border-top:1px solid #202020;font-family:'Roboto', sans-serif !important;}
.top-footer a{font-size:13px;color:#818181;vertical-align:bottom}
.top-footer a:hover{color:#FFF}
.top-footer span{padding:0 10px;color:#4f4f4f}
.center-section{max-width:1346px;margin: 20px auto 50px auto;}
.text-center{text-align:center}
.lead-txt{color:#242424}
.lead-head{font-size: 43px;padding:10px 0;line-height: 45px;display:inline;}
.lead-head h1{font-size:34px;display:inline;}
.lead-head a {padding-right:5px;}
.one-liner h3{font-weight:400;}
.one-liner h3 a{color:#000;line-height:22px;display:inline;padding-right:5px;}
.mini-lead-block{padding:20px 0;border-bottom:1px solid #e3e3e3;border-top:1px solid #e3e3e3}
.list-story h3 a{margin:0;padding:0;line-height:22px;display:block;color:#444;font-weight:400}
.list-story h3{overflow:hidden;font-weight:400;line-height:22px;font-size:20px}
.h-line-head{width:100%}
