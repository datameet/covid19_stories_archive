.h-line-head h2 span{background:#fff;padding:0 10px 0 0}
.h-line-head h2 span a{color:#000}

.h-line-head-eng h2{width:100%;border-bottom:1px solid #e3e3e3;line-height:0;margin:24px 0;font-size:24px;color:#000;font-weight:700}
.h-line-head h2-eng span{background:#fff;padding:0 10px 0 0}
.h-line-head h2-eng span a{color:#000}


